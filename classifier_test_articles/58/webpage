<script>
    function sticktothetop() {
        var window_top = jQuery(window).scrollTop();
        var top = jQuery('#share-anchor').offset().top;
        var left = jQuery('#share-anchor').offset().left;
        if (window_top > top) {
            jQuery('#side-share').addClass('stick');
            jQuery('#side-share').css("left", left - 32);
            jQuery('#share-anchor').height(jQuery('#stickThis').outerHeight());
        } else {
            jQuery('#side-share').removeClass('stick');
            jQuery('#side-share').css("left", "");
            jQuery('#share-anchor').height(0);
        }
    }
    jQuery(function() {
        jQuery(window).scroll(sticktothetop);
        sticktothetop();
    });
</script>
