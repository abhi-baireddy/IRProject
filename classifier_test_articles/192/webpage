  @media(min-width: 600px) {
    .electionNavbar__logoSvg {
      width: 100px;
    }
  }

  .nytslm_notification {
    border-left: 1px solid #ccc;
    font-family: nyt-franklin, helvetica, arial, sans-serif;
    padding-left: 1em;
  }

  .nytslm_notification_label {
    color: #D0021B;
    text-transform: uppercase;
    font-weight: 700;
    font-size: 11px;
    margin-bottom: 0.2em;
    letter-spacing: 0.02em;
  }

  .nytslm_notification_link {
    font-weight: 600;
    color: #121212;
  }

  .nytslm_notification_headline {
    font-size: 14px;
    line-height: 17px;

  }

  .nytslm_notification_headline:after {
    display: inline-block;
    content: '';
    height: 0.3em;
    width: 0.3em;
    margin-bottom: 1px;
    border-top: 2px solid #aaa;
    border-right: 2px solid #aaa;
    transform: rotate(45deg);
  }

  /* No hover state on in app */
  .Hybrid .nytslm_li a:hover,
  .Hybrid .nytslm_li_loud a:hover {
    border-bottom: none;
    padding-bottom: 0;
  }
</style>

