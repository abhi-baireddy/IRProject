var _removed=!1;var form_to_submit=document.getElementById('_form_3_');var allInputs=form_to_submit.querySelectorAll('input, select, textarea'),tooltips=[],submitted=!1;var getUrlParam=function(name){var regexStr='[\?&]'+name+'=([^&#]*)';var results=new RegExp(regexStr,'i').exec(window.location.href);return results!=undefined?decodeURIComponent(results[1]):!1};for(var i=0;i<allInputs.length;i++){var regexStr="field\[(\d+)\]";var results=new RegExp(regexStr).exec(allInputs[i].name);if(results!=undefined){allInputs[i].dataset.name=window.cfields[results[1]]}else{allInputs[i].dataset.name=allInputs[i].name}
