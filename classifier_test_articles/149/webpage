<script>
	jQuery(document).ready(function($){
	    var currentPageTitle = $('.post h1 a').text();
		var currentPage = window.location.href;
		$('.fburl').attr('href', 'https://www.facebook.com/sharer/sharer.php?u='+currentPage);
		$('.twurl').attr('href', 'https://twitter.com/home?status='+currentPageTitle+' '+currentPage);
		$('.socicon-facebook').click( function() {
	        winWidth = 520;
	        winHeight = 350;
	        var winTop = (screen.height / 2) - (winHeight / 2);
			var winLeft = (screen.width / 2) - (winWidth / 2);
			var currentPageTitle = $('.post h1 a').text();
			var currentPage = window.location.href;
		window.open('https://www.facebook.com/sharer/sharer.php?u=' + currentPage, 'sharer', 'top=' + winTop + ',left=' + winLeft + ',toolbar=0,status=0,width=' + winWidth + ',height=' + winHeight);
	    });
	    $('.socicon-twitter').click( function() {
	        winWidth = 520;
	        winHeight = 350;
	        var winTop = (screen.height / 2) - (winHeight / 2);
			var winLeft = (screen.width / 2) - (winWidth / 2);
			var currentPageTitle = $('.post h1 a').text();
			var currentPage = window.location.href;
		window.open('https://twitter.com/home?status=' + currentPageTitle + ' ' + currentPage, 'sharer', 'top=' + winTop + ',left=' + winLeft + ',toolbar=0,status=0,width=' + winWidth + ',height=' + winHeight);
	    });
	});
</script></div>
