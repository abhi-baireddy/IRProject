<source srcset="https://static.politico.com/dims4/default/59766f5/2147483647/resize/646x/quality/90/?url=https%3A%2F%2Fstatic.politico.com%2Fa7%2F05%2Fb921b33247a6bf0a9b353592e03c%2F190227-kamala-gty-773.jpg" media="(min-width: 485px)">
<source srcset="https://static.politico.com/dims4/default/31bd992/2147483647/resize/463x/quality/90/?url=https%3A%2F%2Fstatic.politico.com%2Fa7%2F05%2Fb921b33247a6bf0a9b353592e03c%2F190227-kamala-gty-773.jpg" media="(max-width: 484px)">
<img src="https://static.politico.com/dims4/default/93f702c/2147483647/resize/1160x%3E/quality/90/?url=https%3A%2F%2Fstatic.politico.com%2Fa7%2F05%2Fb921b33247a6bf0a9b353592e03c%2F190227-kamala-gty-773.jpg" alt="Kamala Harris" title="Kamala Harris"></picture>
