    .siu-ad-close-button {
        position: absolute;
        width: 20px;
        height: 20px;
        right: 0;
        top: 0;
        background-image: url(/App_Themes/Newsmax/images/close.png);
        background-size: 100%;
        cursor: pointer;
        z-index: 999999;
    }

    /* Slide in up Ad - Fixed */
    .siu-ad-fixed {
        position: fixed;
        width: 1024px;
        height: 110px;
        z-index: 999998;
        left: 50%;
        margin-left: -512px;
        bottom: -110px; 
    }

    @media (min-width: 768px) {
        .siu-ad-fixed {
            width: 768px;
            height: 110px;
            margin-left: -384px;
        }
    }

    @media (min-width: 992px) {
