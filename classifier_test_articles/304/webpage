<!-- END ID="STORY-META" CLASS="STORY-META HAS-BYLINE" -->
<div id="storytext" class="storytext storylocation linkLocation">
      <div id="res800413040" class="bucketwrap image large">
            <div class="imagewrap" data-crop-type="">
    <img src="https://media.npr.org/assets/img/2020/01/28/palestinian-protest_wide-438351950605b942f9bee27eb8638851ae7879f3-s1100-c15.jpg" data-original="https://media.npr.org/assets/img/2020/01/28/palestinian-protest_wide-438351950605b942f9bee27eb8638851ae7879f3-s1100.jpg" class="img lazyOnLoad"  alt="" />
        <div class="enlarge-options">
            <a class="enlargelink" href="https://media.npr.org/assets/img/2020/01/28/palestinian-protest_wide-438351950605b942f9bee27eb8638851ae7879f3-s1200.jpg">Enlarge this image</a>
