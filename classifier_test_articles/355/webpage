      container: container,
      storyId: "1851002",
      commentId: "",
      backToStoryUrl: "",
      parentThreadUrl: "",
      commentMode: "story-page-mode"
    });
    DailyKos.Story.initStoryPage({
      storyID: "1851002"
    })
  });
</script>
