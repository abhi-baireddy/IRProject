<script type="text/javascript">
 $(function(){
   // almost all of this, except maybe an init line, gets moved to
   // stories.js when we merge in epics/story_authoring_tool,
   // which is why they are hanging out and not namespaced
   var $comments =  $("#comments");

   _.defer(function(){ CommentApp.initHiddenCommentsClickListener($comments); });

   var opts = ({
     el:               $comments,
     tree:             $(".comment-tree"),
     storyID:          1851002,
     authorID:         1756627,
     commentsCloseAt:  new Date(1556427605000),
     commentsPath:     '/stories/1851002/comments'
   });

   if (!window.showNewComments) {
     DailyKos.Story.initStoryPage(opts);
     DailyKos.Story.initBackboneComments(opts)
   }
 });
</script>

