var floatin_share = document.getElementById('share_footer');
var lastScrollTop = 0;

window.addEventListener("scroll", function(){ // window.addEventListener("scroll"....
   if(window.innerWidth < 767){
	   var st = window.pageYOffset || document.documentElement.scrollTop; // Credits: "https://github.com/qeremy/so/blob/master/so.dom.js#L426"
	   if (st > lastScrollTop){
			floatin_share.style.bottom= "0"
	   } else {
		  floatin_share.style.bottom="-100px";
	   }
	   lastScrollTop = st;
   }
   else {console.log}

}, false);
window.addEventListener("resize", function(){ //  window.addEventListener("resize"....
   if(window.innerWidth > 767){
	   
			floatin_share.style.bottom= "0"   }
   else {
		floatin_share.style.bottom= "-100px" 
   }

}, false);

</script><!-- End User Code --></div>
