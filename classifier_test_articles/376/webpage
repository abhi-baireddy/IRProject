            userLoggedIn = false;
            

        deps.push(userLoggedIn ? 'text!https://thefulcrum.us/core/users/settings.js/?v=' + (+new Date) : 'settings');
        rblms.require(deps, function (_, $, BB, SettingsMethods, Settings, userSettings) {
            
            userSettings = _.isObject(userSettings) ? userSettings : JSON.parse(userSettings);

            userSettings.parent_site &&  
                (Settings.submissionsSites = [{"title": "The Fulcrum", "id": 19309670, "name": "thefulcrum"}]);

            userSettings.base_ssl_domain = 'https://thefulcrum.us';

            Settings = $.extend(true, Settings, userSettings || {});
            Settings.is_paid_domain = true;
            Settings.domain = 'https://www.rebelmouse.com';
            Settings.user || (Settings.user = {});
            Settings.user.is_autorized = userLoggedIn;
            Settings.getparams = {};
            Settings.path_params = {"sections": "voting/", "slug": "florida-felons-voter-registration"};
            Settings.device = 'desktop';

            _.each(Settings.sites, function (site) {
                site.id === Settings.site.id && _.extend(Settings.site, site);
            });
            SettingsMethods.call(Settings);
            rblms.define('richeditor', function () {
                return Settings.parent_site ? Settings.parent_site.permissions.publish_post : false;
            });
            rblms.define('bootstrap', function () {
                var bootstrap = {"warnings": [], "has_more": true, "split_testing": {"utm_campaign": "share,new", "name": "Floating share bar", "query-param": "share", "use_cookie": false, "is_picked_by_query": false, "data-rm-show": "true", "data-rm-created-by": "20065717", "query-value": "new", "id": "4a5416d361dda162bdabdc112bc9f513", "data-rm-created-on": "Tue Nov 19 2019 17:30:46 GMT-0500 (Peru Standard Time)"}, "posts_by_source": {"left-most_read": [{"is_direct_link": false, "subpages": [0, 67244355, 75033063], "tags": ["ohio", "north carolina", "maryland", "alabama", "louisiana", "texas", "georgia", "michigan", "gerrymandering", "~popular_source-pageview", "princeton gerrymandering project", "redistrict network", "take back our republic", "campaign legal center", "league of women voters"], "buckets": [], "original_url": null, "provider_id": 0, "share_url": "https%3A%2F%2Fthefulcrum.us%2Fworst-gerrymandering-districts-example", "post_url": "https://thefulcrum.us/worst-gerrymandering-districts-example", "ctas": {}, "media_html": "\u003cimg lazy-loadable=\"true\" src=\"https://assets.rebelmouse.io/eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpbWFnZSI6Imh0dHBzOi8vYXNzZXRzLnJibC5tcy8yMjA2NTM3OS9vcmlnaW4ucG5nIiwiZXhwaXJlc19hdCI6MTYxNjA5NDc1Mn0.QdU4ccgoP9bqDKnodbzULGi5Ea_kUjxThJ5tLTZ7F1Q/img.png?width=1200\u0026coordinates=0%2C595%2C0%2C555\u0026height=600\" id=\"1908f\" class=\"rm-shortcode\" data-rm-shortcode-id=\"4fa66982855f65986f727a10a69821e4\" data-rm-shortcode-name=\"rebelmouse-image\"   /\u003e", "listicle": {"items": [{"body": "\u003cp\u003e\u003cstrong\u003eFierman: \u003c/strong\u003eOhio's so-called \"snake by the lake\" 9th District, which stretches from Toledo to Cleveland, is so thin and strangely shaped that they actually drove to Lake Erie to monitor sea levels with respect to the contiguity of the district. They are concerned that climate change could make the district non-contiguous and consequently altered in the next round of redistricting.\u003c/p\u003e\u003cp\u003e\u003cstrong\u003eHill:\u003c/strong\u003e Democrat Marcy Kaptur is never far from the shore when traveling her district. Comprising portions of Cuyahoga, Erie, Lorain, Lucas and Ottawa counties and bordering Michigan and Canada via Lake Erie, the narrow district is almost laughable \u2014 though it is probably the best place to visit in Ohio because of the beaches and Cedar Point amusement park.\u003c/p\u003e\u003cp\u003e\u003cstrong\u003eLamar: \u003c/strong\u003eIn May a panel of federal judges in Cincinnati described the district as \"a bizarre, elongated sliver of a district that sever[s] numerous counties.\" The purpose for that nonsensical grouping is to pack as many Democratic voters as possible into a single district. The court held that the 9th is a blatant partisan gerrymander, but there will be no federal judicial remedy since the \u003ca href=\"https://thefulcrum.us/supreme-court-gerrymandering-decision\" target=\"_blank\"\u003eSupreme Court's ruling\u003c/a\u003e in June that the courts have no role in ending partisan gerrymandering. \u003c/p\u003e\u003cp\u003e\u003cstrong\u003eMiller:\u003c/strong\u003e The 9th was designed by the Republican who controlled state mapmaking in 2001 to get rid of a Democratic House member, by making two incumbents living more than 100 miles apart compete for one seat. The economic differences across the district are significant. The scientific industry is the largest employer in Cleveland, then represented by Dennis Kucinich, but science jobs are far less common in Kaptur's Toledo and fairly rare in the rural counties where either agriculture or tourism reign. The cultural differences couldn't be starker: Residents of Toledo are seven times more likely to root for the University of Michigan (Ohio State's rival since 1918) than those in Cleveland. And even tiny communities like Florence, with 2,400 residents, are represented by two different members of Congress. When large cities are packed together and small communities cracked apart, it becomes pretty obvious that constituents are being robbed of real representation.\u003c/p\u003e", "index": 1, "description": "\u003cp\u003e\u003cstrong\u003eFierman: \u003c/strong\u003eOhio's so-called \"snake by the lake\" 9th District, which stretches from Toledo to Cleveland, is so thin and strangely shaped that they actually drove to Lake Erie to monitor sea levels with respect to the contiguity of the district. They are concerned that climate change could make the district non-contiguous and consequently altered in the next round of redistricting.\u003c/p\u003e\u003cp\u003e\u003cstrong\u003eHill:\u003c/strong\u003e Democrat Marcy Kaptur is never far from the shore when traveling her district. Comprising portions of Cuyahoga, Erie, Lorain, Lucas and Ottawa counties and bordering Michigan and Canada via Lake Erie, the narrow district is almost laughable \u2014 though it is probably the best place to visit in Ohio because of the beaches and Cedar Point amusement park.\u003c/p\u003e\u003cp\u003e\u003cstrong\u003eLamar: \u003c/strong\u003eIn May a panel of federal judges in Cincinnati described the district as \"a bizarre, elongated sliver of a district that sever[s] numerous counties.\" The purpose for that nonsensical grouping is to pack as many Democratic voters as possible into a single district. The court held that the 9th is a blatant partisan gerrymander, but there will be no federal judicial remedy since the \u003ca href=\"https://thefulcrum.us/supreme-court-gerrymandering-decision\" target=\"_blank\"\u003eSupreme Court's ruling\u003c/a\u003e in June that the courts have no role in ending partisan gerrymandering. \u003c/p\u003e\u003cp\u003e\u003cstrong\u003eMiller:\u003c/strong\u003e The 9th was designed by the Republican who controlled state mapmaking in 2001 to get rid of a Democratic House member, by making two incumbents living more than 100 miles apart compete for one seat. The economic differences across the district are significant. The scientific industry is the largest employer in Cleveland, then represented by Dennis Kucinich, but science jobs are far less common in Kaptur's Toledo and fairly rare in the rural counties where either agriculture or tourism reign. The cultural differences couldn't be starker: Residents of Toledo are seven times more likely to root for the University of Michigan (Ohio State's rival since 1918) than those in Cleveland. And even tiny communities like Florence, with 2,400 residents, are represented by two different members of Congress. When large cities are packed together and small communities cracked apart, it becomes pretty obvious that constituents are being robbed of real representation.\u003c/p\u003e", "is_image": true, "credit": "\u003cp\u003e\u003ca href=\"https://mapchart.net/\" target=\"_blank\"\u003ehttps://mapchart.net/\u003c/a\u003e\u003c/p\u003e", "headline": "1. Beside Lake Erie", "basename": "1-beside-lake-erie", "post_id": 2640600189, "href": "https://thefulcrum.us/worst-gerrymandering-districts-example?rebelltitem=3#rebelltitem3", "media_html": "\u003cimg lazy-loadable=\"true\" src=\"https://assets.rebelmouse.io/eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpbWFnZSI6Imh0dHBzOi8vYXNzZXRzLnJibC5tcy8yMjA2MTA0OC9vcmlnaW4ucG5nIiwiZXhwaXJlc19hdCI6MTU5NDQ4NDUzN30.YgQFHhPm-FBswRslPNqCtKeGIfJSluYErvqj2e42ZQk/img.png?width=1200\u0026coordinates=1676%2C493%2C460%2C463\u0026height=800\" id=\"1b835\" class=\"rm-shortcode\" data-rm-shortcode-id=\"8cf7375d3cf6cb6632ae871e76344915\" data-rm-shortcode-name=\"rebelmouse-image\"   /\u003e", "id": 3, "numeration": "", "thumbnails": {"origin": "https://assets.rbl.ms/22061048/origin.png", "210x": "https://thefulcrum.us/media-library/eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpbWFnZSI6Imh0dHBzOi8vYXNzZXRzLnJibC5tcy8yMjA2MTA0OC9vcmlnaW4ucG5nIiwiZXhwaXJlc19hdCI6MTU5NDQ4NDUzN30.YgQFHhPm-FBswRslPNqCtKeGIfJSluYErvqj2e42ZQk/image.png?width=210", "980x": "https://thefulcrum.us/media-library/eyJhbGciOiJIUzI1NiIsInR5cCI