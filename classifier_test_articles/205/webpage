  function showPianoLogin () {
      var tp = window.tp || [];
      tp.push(["init", function() {
        tp.pianoId.show({
          screen: 'login',
          loggedIn: function(data) {
              console.log('__PIANO user ', data.user, ' logged in with token', data.token);
              $('#anonUserMenu').hide();
              $('#memberMenu').show();
          }
        });
        $('.tp-modal').css('z-index', '2000000000');
      }]);
  }

  function pianoLogout() {
      console.log('__PIANO: logout');
      var tp = window.tp || [];
      tp.pianoId.logout(function(){
        console.log('__PIANO: logged out');
        $('#anonUserMenu').show();
        $('#memberMenu').hide();
      });
      
  }

  //set initial user account state
  var tp = window.tp || [];
  tp.push(["init", function() {

    if (tp.pianoId.isUserValid()) {
      $('#memberMenu').show();
    } else {
      $('#anonUserMenu').show();
    }

  }]);

