    <script id="android-detector">
        function getAndroidVersionFloat(){try{var a=getAndroidVersion();if(isValidAndroidVersion(a)){return parseFloat(a)}}catch(e){}return 0}function getAndroidVersion(){var a=null;try{if(isAndroid()){a=getAndroidVersionOne();if(isValidAndroidVersion(a)){return a}else{a=getAndroidVersionTwo();if(isValidAndroidVersion(a)){return a}}}}catch(e){}return a}function isAtLeastAndroidVersion(a){try{if(isAndroid()){if(a<=getAndroidVersionFloat()){return true}else{return false}}}catch(e){}return false}function isValidAndroidVersion(a){try{if(a==null){return false}return isNaN(parseFloat(a))===false}catch(e){}return false}function isAndroid(a){try{var b=(a||navigator.userAgent).toLowerCase();var c=b.match(/android/);return c?true:false}catch(e){}return false}function getAndroidVersionOne(a){try{var b=(a||navigator.userAgent).toLowerCase();var c=b.match(/android\s([0-9\.]*)/);return c?c[1]:null}catch(e){}return null}function getAndroidVersionTwo(){try{if(isAndroid()){var a=(a||navigator.userAgent).toLowerCase();var b=a.match(/release\/(.*)/);return b?b[1]:null}}catch(e){}return null}
    </script>
