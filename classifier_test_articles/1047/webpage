    } else {
        // Other pages
        function sticky_relocate() {
            var window_top = $( window ).scrollTop();
            var div_top = $('.sticky-wrapper').length ? $('.sticky-wrapper').offset().top + 500 : 0;
            var sidebar_height = $( '#article-sidebar' ).height();
            var sidebar_bottom = $( '#article-sidebar' ).offset().bottom;


            //if (window_top > div_top) {
            if (window_top > sidebar_height ) {
                var sidebar_height = $( '#article-sidebar' ).height();
                $('#sticky-anchor').height( $('#sticky').outerHeight() + 25 );
                //$('#sticky').css( 'top', ( ( window_top - sidebar_height ) + 600 ) + 'px' );
                var _offset = $( '#div-gpt-728x90_1' ).height() < 100 ? ( window_top - sidebar_height ) + 40 : window_top - sidebar_height - 120;
                if( _admin_bar.length ) _offset = _offset + _admin_bar.height();
                $('#sticky').css( 'top', _offset + 'px' );
            } else {
                jQuery('#sticky-anchor').height(25);
                //$('#sticky').css( 'position', 'absolute' ).css( 'top', '-30px' );
                //$('#sticky').css( 'top', '-30px' );
                $('#sticky').css( 'top', '0px' );
            }

        }

        $( window ).scroll( sticky_relocate );
        

    }
})( window.jQuery );
</script>


