                    <a class="logo" href="/illinois/" data-title="Cook County Judge Loosens Unusual Restrictions on Publishing Details of Child Welfare Case">
                        <svg class="logo-main" id="propublica-illinois-wordmark" data-name="propublica-illinois-wordmark" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 342.62 100"><defs></defs><title>ProPublica Illinois logo</title><path d="M128.39,35.33l4.28.51v2h-12.3v-2l2.47-.64V14.62L120.37,14v-1.9h11.07c7.16,0,10.72,2.47,10.72,7.54,0,5.23-3.91,8.15-10.82,8.15h-3Zm0-9.89h3.21c3,0,4.91-2.16,4.91-5.65s-1.74-5.36-4.72-5.36h-3.4Z"/><path d="M167.81,35.84v2h-6.95l-7.48-9.64h-1.17v7l2.47.64v2h-10.5v-2l2.48-.64V18.14l-2.35-.63v-1.9h11.54c4.92,0,8.22,2.37,8.22,5.93,0,3.93-2.7,5.7-5.14,6.34l6.31,7.39Zm-15.6-10h2.09c2.69,0,4.12-1.49,4.12-4.16,0-2.5-1.33-3.71-4.12-3.71h-2.09Z"/><path d="M167.27,26.7c0-6.91,4.6-11.57,12-11.57s12,4.66,12,11.57-4.6,11.58-12,11.58S167.27,33.65,167.27,26.7Zm18.2,0c0-4.53-1.52-9-6.19-9s-6.18,4.44-6.18,9,1.52,9,6.18,9S185.47,31.24,185.47,26.7Z"/><path d="M200.59,35.33l4.28.51v2H192.41v-2L195,35.2V14.62L192.41,14v-1.9h11.23c7.16,0,10.71,2.47,10.71,7.54,0,5.23-3.9,8.15-10.81,8.15h-2.95Zm0-9.89h3.21c3,0,4.91-2.16,4.91-5.65S207,14.43,204,14.43h-3.4Z"/><path d="M239.66,17.51l-2.32.63V29.21c0,6-3.64,9.07-9.38,9.07-6.38,0-9.71-3.68-9.71-9.36V18.14l-2.31-.63v-1.9h10.31v1.9l-2.38.63V29.56c0,3.39,2,5.64,5.36,5.64a5,5,0,0,0,5.26-5.29V18.14l-2.38-.63v-1.9h7.55Z"/><path d="M262.87,31.65c0,4.15-3.33,6.15-10.31,6.15H241.21v-2l2.6-.64V18.14l-2.28-.63v-1.9h11.19c5.9,0,9.13,1.77,9.13,5.48,0,2.89-1.9,4.25-5.1,4.63v.13C260.74,26.29,262.87,28.32,262.87,31.65Zm-13.51-6.5h2.7c2.85,0,4.21-1.3,4.21-3.65S254.81,18,252.06,18h-2.7Zm7.86,6.28c0-2.63-1.77-4.06-5.26-4.06h-2.6v8.09h3.11C255.48,35.46,257.22,34,257.22,31.43Z"/><path d="M285,30l-2.06,7.83H264.8v-2l2.48-.64V18.14l-2.64-.63v-1.9H275.3v1.9l-2.47.63V35.46l6.62-.38,3.62-5.74Z"/><path d="M296.35,35.84v2H285.86v-2l2.47-.64V18.14l-2.47-.63v-1.9h10.49v1.9l-2.47.63V35.2Z"/><path d="M297.59,26.77c0-7.42,5.67-11.64,12.65-11.64a16.67,16.67,0,0,1,8.56,2.41l-.95,6.18h-2l-.8-4.25a6.18,6.18,0,0,0-4.72-1.93c-4.28,0-6.88,3.08-6.88