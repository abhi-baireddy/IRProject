<script>
(function(){
  function loadSafeFrame() {
    if (window.OSafeframe && window.OSafeframe.initNPS) {
      return Promise.resolve();
    } else {
      return new Promise(function(resolve) {
        var script = document.createElement('script');
        script.defer = true;
        script.type = 'text/javascript';
        script.src = 'https://s.yimg.com/uc/sf/latest/js/safe.min.js';
        script.onload = function() {
          waitForGlobal(function() {
            return window.OSafeframe && window.OSafeframe.initNPS;
          }, resolve);
        };
        (document.getElementsByTagName('head')[0] ||
          document.getElementsByTagName('body')[0]).appendChild(script);
      });
    }
  }
  // The `delay` property of the initNPS config won't work
  // if it gets invoked after the onload event, so apply a
  // more specific delay here.
  var delayedLoad = new Promise(function(resolve, reject) {
    // The NPS module uses the window load event, but the page
    // frequently has resources hang seemingly forever and delay
    // onload, because that's how HuffPost rolls I guess.
    document.addEventListener("DOMContentLoaded", function() {
      setTimeout(resolve, 5000);
    });
  });
  if (typeof HP !== "undefined") {
    waitForGlobal(function() {
      return !!(window.HP && window.HP.modules);
    }, function() {
      var sessionCount = HP.modules.cohorts.total('session');
      var npsSetting = HP.modules.storage.get('nps-2020-UXR');
      var num = HP.modules.storage.get('npsNum-2020-UXR') || Math.random() * 100;
      HP.modules.storage.set('npsNum-2020-UXR', num);
      var inGroup = num <= 0.03;
      console.log('inGroup: ' + inGroup);
      console.log('sessionCount: ' + sessionCount);
      console.log('npsSetting: ' + npsSetting);
      console.log('num: ' + num);
      if(sessionCount > 1 && npsSetting !== true && inGroup) {
        loadSafeFrame().then(function(){return delayedLoad;}).then(function(){
          HP.modules.storage.set('nps-2020-UXR', inGroup);
          window.OSafeframe.initNPS('nps-container', {
            surveyid: 'CTCYT3H', // required
            lang: 'en', //optional
          }, {
            pageLocale: 'en' // used for localizing the Close button. can be skipped if surveylang is defined, or en (or en-XX) is the language
          });
        });
      }
    });
  }
}());
