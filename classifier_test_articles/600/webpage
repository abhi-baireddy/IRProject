ams":{},"path":"","pathname":"","query":{}},"routed":false,"scrollToPostComments":null,"searchResultIDs":[],"seriesHomepage":{"curatedItems":{"postIds":[],"seriesSlugs":[]},"recentItems":{"postIds":[],"seriesSlugs":[]}},"seriesPostIDs":{},"sidToday":{"search":{"lastCursor":null,"loading":false,"speakingIDs":[],"totalCount":null}},"sidTodayFilesUpdateReports":{"category":{}},"specialSeriesItems":[],"squirrelDocumentIDs":[],"squirrelIDs":[],"staffDetailIDs":{},"staffIDs":[],"surveillanceCatalogData":null,"surveillanceCatalogVendors":null,"tracking":{"currentUrl":null,"previousUrl":null}}
