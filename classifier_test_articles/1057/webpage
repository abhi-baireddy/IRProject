  if( typeof pjm.featuredimagebuddy.initialize != 'undefined' ) pjm.featuredimagebuddy.initialize();
  // these JS have popups, this ensures the right order
  if( typeof pjm.desktopnotifications != 'undefined' && typeof pjm.desktopnotifications.initialize != 'undefined' ) pjm.desktopnotifications.initialize("BNCGxKiFa1WzCUKQ8uPvlED1Rg83-8yqVxvTuCRMw0IoRKg4fzaW0bB7C2mk43SDEvD10dJ_zmHuCsR2p0fBni8=");
  if( typeof pjm.newsletters != 'undefined' && typeof pjm.newsletters.initialize != 'undefined' ) pjm.newsletters.initialize();
  // end popups
})( window.jQuery );
</script>

