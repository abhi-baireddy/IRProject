    <a href="/media-places/washington-post" typeof="skos:Concept" property="rdfs:label skos:prefLabel" datatype="" class="btn btn-default">Washington Post</a>
    <a href="/people-organizations/tara-reade" typeof="skos:Concept" property="rdfs:label skos:prefLabel" datatype="" class="btn btn-default">Tara Reade</a>
    <a href="/journalists/dean-baquet" typeof="skos:Concept" property="rdfs:label skos:prefLabel" datatype="" class="btn btn-default">Dean Baquet</a>
    <a href="/non-journalists/joe-biden" typeof="skos:Concept" property="rdfs:label skos:prefLabel" datatype="" class="btn btn-default">Joe Biden</a>
