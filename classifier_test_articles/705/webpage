			pbjs.bidderSettings = {
				standard: {
					bidCpmAdjustment: function( bidCpm, bid ) {
						let screen_size = window.innerWidth < 944 ? 'SMSX' : 'LGSX';
						let factor = 1.0;
						if( typeof FFADS.units[bid.adUnitCode].scales[screen_size][bid.bidderCode] != 'undefined' ){
							factor -= FFADS.units[bid.adUnitCode].scales[screen_size][bid.bidderCode];
							FFADS.debug( 'factor for ' + bid.bidderCode + ' in ' + bid.adUnitCode + ' set to ' + factor + '; bid changed from ' + bidCpm + ' to ' + ( bidCpm *= factor ) );
						}
						return bidCpm;
					}
				}
