			FFADS.render_ad = function( ad_unit, div_id = false ) {
				div_id = div_id || ad_unit;
				this.debug( 'refreshing ' + ad_unit + ' into #' + div_id );
				this.units[ad_unit].instances[div_id].displayed = true;
