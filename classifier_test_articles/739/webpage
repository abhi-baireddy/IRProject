		//whenever you want header bids call this function
		function fetchHeaderBids(amazonSlots, pbSlots, iasSlots) {
			var opt = arguments.length > 3 && arguments[3] !== undefined ? arguments[3] : false;

			var bidTimeout = 1000;

			var bidders = ['a9', 'prebid', 'ias'];

			// create a requestManager to keep track of bidder state to determine when to send ad server
			// request and what amazonSlots to request from the ad server
			var requestManager = {
				adserverRequestSent: false,
			};

			//loop through bidder array and add the bidders to the request manager:
			bidders.forEach(function(bidder) {
				requestManager[bidder] = false;
			})


			// a helper function that returns googletag objects corresponding to the slotIDs in the slot objects
			function _getGPTSlots(amazonSlots) {
				// get all of the slot IDs that were requested
				var slotIDs = amazonSlots.map(function(slot) {
					return slot.slotID;
				});

				// return the slot objects corresponding to the slotIDs
				return googletag.pubads().getSlots().filter(function(slot) {
					// return true if the ID of the slot object is in the slotIDs array
					return slotIDs.indexOf(slot.getSlotElementId()) > -1;
				});

			}


			// return true if all bidders have returned
			function allBiddersBack() {
				var allBiddersBack = bidders
				// get the booleans from the object
					.map(function(bidder) {
						return requestManager[bidder];
					})
					// get rid of false values - indicates that the bidder has responded
					.filter(Boolean)
					// if length is equal to bidders, all bidders are back
					.length === bidders.length;

				return allBiddersBack;
			}


			// handler for header bidder responses
			function headerBidderBack(bidder) {
				// return early if request to adserver is already sent
				if (requestManager.adserverRequestSent === true) {
					return;
				}
				// flip bidder back flag
				if (bidder === 'a9') {
					requestManager.a9 = true;
				} else if (bidder === 'prebid') {
					requestManager.prebid = true;
				} else if ( bidder === 'ias' ) {
					requestManager.ias = true;
				}

				// if all bidders are back, send the request to the ad server
				if (allBiddersBack()) {
					sendAdserverRequest();
				}
			}

			// actually get ads from DFP
			function sendAdserverRequest() {

				// return early if request already sent
				if (requestManager.adserverRequestSent === true) {
					return;
				}

				// flip the boolean that keeps track of whether the adserver request was sent
				requestManager.adserverRequestSent = true;
				// flip pbjs boolean to signal to pbjs the ad server has already been called
				pbjs.adserverRequestSent = true;

				//flip boolean for adserver request to avoid duplicate requests
				requestManager.sendAdserverRequest = true;

				// make ad request to DFP
				googletag.cmd.push(function() {
					apstag.setDisplayBids();
					pbjs.setTargetingForGPTAsync();
					__iasPET.setTargetingForGPT();
					window.googletag.pubads().refresh(filterAdSlots(pbSlots), {changeCorrelator: opt});
				});
			}

			function contains(arr,obj) {
				return (arr.indexOf(obj) !== -1);
			}

			function filterAdSlots(slotIdArray) {
				return window.googletag.pubads().getSlots()
					.filter(function(slot) {
						return contains(slotIdArray, slot.getAdUnitPath()) || contains(slotIdArray, slot.getSlotElementId());
					});
			}

			function requestBids(pbSlots) {
				// request bids from prebid
				pbjs.que.push(function () {
					pbjs.initAdserverSet = true;
					pbjs.requestBids({
						adUnitCodes: pbSlots,
						bidsBackHandler: function bidsBackHandler() {
							headerBidderBack('prebid');
						}
					});
				});
			}

			if(amazonSlots.length) {
				apstag.fetchBids({
					slots: amazonSlots,
					timeout: bidTimeout
				}, function (bids) {
					headerBidderBack('a9');
				});
			} else {
				headerBidderBack('a9');
			}

			//requestBids
			pbjs.que.push( function() {
				requestBids(pbSlots)
			} );

			// make the request to PET. if your page makes multiple ad requests to DFP // (e.g., lazily loaded ads, infinite scrolling pages, etc.), you should make // a request to PET before every request to DFP
			__iasPET.queue.push( {
				adSlots: iasSlots,
				dataHandler: headerBidderBack( 'ias' )
			} );

			// set timeout to send request to call sendAdserverRequest() after timeout
			// if all bidders haven't returned before then
			var bidding = setTimeout(function() {
				sendAdserverRequest();
			}, 1500);
		};

		function customInfiniteScroll( mapping, slotID, aSlot, iasSlot, opt ) {
			var codes = [];
			var configAdUnits = window.adUnits;
			var infiniteScrollMap = {};
			if ( mapping.length > 0 && typeof window.adUnits !== 'undefined' ) {
				for ( var i = 0; i < mapping.length; i++ ) {
					infiniteScrollMap[mapping[i].targetAdUnitCode] = mapping[i].newAdUnitCode;
					codes.push( mapping[i].newAdUnitCode );
				}
				for ( var i = 0; i < configAdUnits.length; i++ ) {
					var val = infiniteScrollMap[configAdUnits[i].code];
					if ( typeof val !== "undefined" ) {
						var oldSlot = configAdUnits[i];
						oldSlot.code = infiniteScrollMap[configAdUnits[i].code];
						pbjs.removeAdUnit( configAdUnits[i].code );
						configAdUnits[i] = oldSlot;
					}
				}
				pbjs.addAdUnits( configAdUnits );
			}
			fetchHeaderBids( aSlot, [slotID], iasSlot, opt );
		}

		googletag.cmd.push(function(){
			var firstAdRequest = new Date().getTime();
			console.log('AMI GPT: first Ad Request', firstAdRequest);
			fetchHeaderBids(amazonSlots, initSlots, window.iasPETSlots);
		});


		/**
		 * GPT helpers functions.
		 */

		var ami_gpt = (function () {

			var elementInViewport = function (el) {
				var rect = el.getBoundingClientRect();
				return (rect.top >= 0 && rect.left >= 0 && rect.top <= (window.innerHeight + (window.innerHeight / 2) || document.documentElement.clientHeight + (window.innerHeight / 2)))
			};

			var elementInViewportStrict = function (el) {
				var rect = el.getBoundingClientRect();
				return (rect.top >= 0 && rect.left >= 0 && rect.top <= (window.innerHeight || document.documentElement.clientHeight))
			};

			var init = function () {
				window.addEventListener('scroll', processElements);
				window.addEventListener('touchstart', processElements);
			};

			var processElements = function () {
				if (typeof pbjs.initAdserverSet === 'undefined') {
					return;
				}
				Object.keys(llSlots).forEach(function (key) {
					var el = document.getElementById(key);
					var correlator = false;
					var aSlot = [];
					var iasSlot = [];
					if (el !== null && elementInViewport(el)) {
						googletag.cmd.push(function () {
							googletag.slots[key] = googletag.defineSlot(adUnitPath, llSlots[key].size, key).defineSizeMapping(sizeMapping[llSlots[key].mapping]).setTargeting('pos', llSlots[key].name).addService(googletag.pubads());
							if(typeof llSlots[key].targeting !== 'undefined') {
								Object.keys(llSlots[key].targeting).forEach(function (tkey) {
									googletag.slots[key].setTargeting(tkey, llSlots[key].targeting[tkey]);
								});
							}
							if(typeof llSlots[key].correlator !== 'undefined') {
								correlator = true;
							}
							googletag.display(key);
							iasSlot.push( {adSlotId: key, size: llSlots[key].size, adUnitPath: window.gptNetwork + key} );
							if(-1 === amazonOff.indexOf(llSlots[key].name)) {
								aSlot.push({'slotID' : key, 'slotName' : llSlots[key].name, 'sizes' : llSlots[key].size});
							}

							if(llSlots[key].name in infSlots) {
								console.log('AMI: infslot ', infSlots[llSlots[key].name], key);
								customInfiniteScroll([
									{
										targetAdUnitCode: infSlots[llSlots[key].name],
										newAdUnitCode: key
									}
								], key, aSlot, correlator);
								infSlots[llSlots[key].name] = key;
							} else {
								console.log('AMI: slot ', aSlot, key);
								fetchHeaderBids(aSlot, [key], iasSlot, correlator);
							}
							console.log('AMI: lazyloaded: ' + key);
						});
						delete llSlots[key];
					}
				});
			};

			var refreshSlot = function (slot_id) {
				if (typeof pbjs.initAdserverSet === 'undefined') {
					return;
				}
				var el = document.getElementById(slot_id);
				if (el !== null && elementInViewportStrict(el) && window.adsRefresh.impr[slot_id] === true) {
					clearInterval(window.adsRefresh.ads[slot_id]);
					window.adsRefresh.impr[slot_id] = false;
					console.log('AMI: refreshed: ',slot_id);
					var iasSlot = [{adSlotId: slot_id, size: googletag.slots[slot_id].ad_sizes, adUnitPath: window.gptNetwork + slot_id}];
					if(typeof amazonSlotsRef[slot_id] !== 'undefined') {
						fetchHeaderBids([amazonSlotsRef[slot_id]], [slot_id], iasSlot, false);
					} else {
						fetchHeaderBids([], [slot_id], iasSlot, false);
					}
				}

			};


			return {
				init: init(),
				processElements: processElements(),
				refreshSlot: refreshSlot
			}

		})();
	</script>
		<script>
		window.refresh = false;
		if ( cType == 'GalleryPost' && window.refresh && window.innerWidth > 1023 ) {
			window.refresh = false;
		}
		if ( window.refresh ) {
			googletag.cmd.push( function() {
				googletag.pubads().addEventListener( 'slotRenderEnded', function( event ) {
					var slot_id = event.slot.getSlotElementId();
					console.log( 'AMI: rendered: ', slot_id );
					if ( -1 === window.adsRefresh.not.indexOf( slot_id ) ) {
						if ( event.isEmpty ) {
							window.adsRefresh.impr[slot_id] = true;
							window.adsRefresh.ads[slot_id] = setInterval( function() {
								ami_gpt.refreshSlot( slot_id );
							}, window.adsRefresh.time );
							window.dataLayer = window.dataLayer || [];
							dataLayer.push( {event: 'adsSlotEmpty', eventLabel: slot_id,} );
						}
					}
				} );
				googletag.pubads().addEventListener( 'impressionViewable', function( event ) {
					var slot_id = event.slot.getSlotElementId();
					console.log( 'AMI: imr: ', slot_id );
					if ( -1 === window.adsRefresh.not.indexOf( slot_id ) ) {
						console.log( 'AMI: will be refreshed: ', slot_id );
						window.adsRefresh.impr[slot_id] = true;
						window.adsRefresh.ads[slot_id] = setInterval( function() {
							ami_gpt.refreshSlot( slot_id )
						}, window.adsRefresh.time );
					} else {
						console.log( 'AMI: will not be refreshed: ', slot_id );
					}
				} );
			} );
		}
