			(function ($) {
				"use strict";
				$(function () {

					/**
					 * Process the for submit event.
					 *
					 * @return {void}
					 */
					var processSubmit = function( $form ) {
						var user_input = $form.serialize();

						$.post(
							ajaxurl,
							user_input,
							function(data) {
								data = jQuery.parseJSON(data);
								if( data.error == true ) {
									$(".postup-add-subscriber-errors").html(data.message);
								} else {
									$($form).html('');
									$($form).parent().find(".success").show();
								}
							}
						);
					};

					// when a user clicks subscribe
					$(".postup-add-subscriber-form").submit( function( e ){
						var $form = $(this);

						e.preventDefault();
						processSubmit( $form );

					});

				});
			}(jQuery));
		</script>

