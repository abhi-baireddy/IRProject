var disqusCt = '<div id="disqus_thread"></div>',
    disqus_config;

rblms.require(['jquery'], function($) {

/*Add comment Button to all articles*/
function addComments () {
  var processedClass = 'js--has-comments',
      $sharesPanel = $('.share-panel:not(.'+processedClass+')');

  $.each($sharesPanel, function(){
    var $t = $(this).addClass(processedClass),
        $thisPost = $t.closest('.widget');
    
    if($t.closest('.article__shares').length) {
        var postId = __BOOTSTRAP__.post.id,
            postURL = location.protocol + '//' + location.host + location.pathname;
        addButton(postURL, postId, $t);
     } else if( $thisPost.length ) {
        var postId = $thisPost.attr('elid'),
            postURL = $thisPost.find('.widget__headline-text').attr('href');
        addButton(postURL, postId, $t);
     }
     
  });
}

addComments();

(function() {
  var d = document, s = d.createElement('script');
  s.src = 'https://wonkettecom.disqus.com/embed.js';
  s.setAttribute('data-timestamp', +new Date());
  (d.head || d.body).appendChild(s);
})();


(function() {
var s = document.createElement('script');
s.type = 'text/javascript';
s.async = true;
s.id = 'dsq-count-scr';
s.src = "//wonkettecom.disqus.com/count.js";
var x = document.getElementsByTagName('script')[0];
x.parentNode.insertBefore(s, x);
})();


function addButton(url, id, el) {
    var commentBtn = '<a class="js--toggleDisqus disqus_comments_count" data-posturl="'+url+'" data-disqus-url="'+url+'" data-postid="'+id+'" href="javascript:;">0</a>';
    el.append(commentBtn);
}

$('body').on('click', '.js--toggleDisqus, .show-more', function (){

  var $t = $(this),
      $thisPost = $t.parents('article').first() || $t.closest('.js-section-loader-feature-container'),
      postId = $t.attr('data-postid'),
      postURL = $t.attr('data-posturl');

  if ( $thisPost.find('#disqus_thread').length ) {
   $('#disqus_thread').remove();
   return false;
  }

  $('#disqus_thread').remove();
  if( $t.closest('.article__shares').length ) {
     $('#FirstJar').after(disqusCt);
  } else {
     $t.closest('.mb-2').next('.tip-jar').after(disqusCt);
 }

  console.log('DISQUS.reset');
  console.log('DISQUS.reset', postId, postURL);
  DISQUS.reset({
    reload: true,
    config: function () {
      this.page.identifier = postId;
      this.page.url = postURL;
    }
  });

if( $t.hasClass('js--toggleDisqus') ) {
      $("html, body").animate({
        scrollTop: $('#disqus_thread').offset().top - 200
      }, 1000);
}
});

window.addEventListener('load-more-event', addComments);
});
