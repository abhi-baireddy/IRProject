<script type="text/javascript" defer>
jQuery(document).ready(function() {
  jQuery('#nav-share').hide();

  jQuery('body').scrollspy({
    target: '#reading-list-nav'
  });



  // update the nav-share bar with current article info
  jQuery("#reading-list-nav").on('activate.bs.scrollspy', function() {
    var currentArticle = jQuery("#reading-list-nav .nav li.active > a").text();
    var currentUrl = jQuery("#reading-list-nav > ol > li.active > a").data("url");

	var share_link_fb = "http://www.facebook.com/sharer/sharer.php?u="+currentUrl+"&title="+currentArticle+'"';
	var share_link_tw = "http://twitter.com/share?text="+currentArticle+"&via=twitchyteam&url="+currentUrl+'"';


    jQuery(".article-nav-headline").html(currentArticle);
    jQuery("#nav-share-tw").attr('href', share_link_tw);
    jQuery("#nav-share-tw").attr('data-title', currentArticle);
    jQuery("#nav-share-fb").attr('href', share_link_fb);
    jQuery("#nav-share-fb").attr('data-title', currentArticle);


	if ( window.location.href != currentUrl ) {
		history.pushState( null, null, currentUrl );
	}

  })

});

//hide navshare if the reading list is not on the page
jQuery(document).scroll(function() {

	jQuery('#top-share-buttons').on('inview', function(event,isInView) {
		if (isInView) {
		    jQuery('#nav-share').hide();
		    jQuery('.thm-carousel').hide();
		} else {
		    jQuery('#nav-share').show();
		}
	});
	jQuery('#townhall-trending').on('inview', function(event,isInView) {
		if (isInView) {
		    jQuery('#article-sticky-ad').hide();
		    console.log('hide article sticky ad');
		} else {
		    jQuery('#article-sticky-ad').show();
		}
	});



});

</script>

