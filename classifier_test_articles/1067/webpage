<source srcset="https://static.politico.com/dims4/default/c5ffc19/2147483647/resize/646x/quality/90/?url=https%3A%2F%2Fstatic.politico.com%2F4f%2F68%2Fb53f7b43451ba5050eb1647fb19b%2F190318-nunes-gty-773.jpg" media="(min-width: 485px)">
<source srcset="https://static.politico.com/dims4/default/6d6b6ad/2147483647/resize/463x/quality/90/?url=https%3A%2F%2Fstatic.politico.com%2F4f%2F68%2Fb53f7b43451ba5050eb1647fb19b%2F190318-nunes-gty-773.jpg" media="(max-width: 484px)">
<img src="https://static.politico.com/dims4/default/0ffe855/2147483647/resize/1160x%3E/quality/90/?url=https%3A%2F%2Fstatic.politico.com%2F4f%2F68%2Fb53f7b43451ba5050eb1647fb19b%2F190318-nunes-gty-773.jpg" alt="Rep. Devin Nunes" title="Rep. Devin Nunes"></picture>
