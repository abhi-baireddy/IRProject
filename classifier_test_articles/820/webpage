			<script>
				function getCookie(){
					var CookieDate = new Date;
					CookieDate.setFullYear(CookieDate.getFullYear() + 1);
					document.cookie = 'mng_privacy_accept=1; expires=' + CookieDate.toUTCString() + ';' + 'path=/';
					document.getElementById("cookie-notice").style.visibility="hidden";
					  if (window.innerWidth < 1025) {
						document.getElementById("mobile-adhesion").style.display = "block";
					  }
				}
					document.cookie.indexOf("mng_privacy_accept") < 0 && (document.getElementById("cookie-notice").style.visibility="visible");
			</script>
			
