    (function () {
        if (s.prop4 !== 'searchresultspage') {
            var s_code = s.t();
            if (s_code) document.write(s_code);

            return;
        }

        $.subscribe('searchResultsReady', trackSearchResults);

        function trackSearchResults () {
            $.unsubscribe('searchResultsReady', trackSearchResults);
            var searchRes = document.getElementById("resultsCount");
            if (searchRes) {
                var resVal = document.getElementById("resultsCount").innerHTML;
                if (resVal == "0") {
                    window.s.events = 'event3,event12';
                } else {
                    s.prop16 = resVal;
                    window.s.events = 'event3,event11';
                }
            }

            var s_code = s.t();
            if (s_code) document.write(s_code);
        }
    })();
</script>
