    if (document.referrer) {
        var referrerElement = document.createElement('a');
        referrerElement.href = document.referrer;

        var referrerHostName = referrerElement.host.split(':')[0];

        if ($.inArray(referrerHostName, paid_traffic) > -1 || $.inArray(referrerHostName.replace(/^www./, ""), paid_traffic) > -1) {
                parselySiteID = 'paid.bet.com';
        }

    }

    if (parselySiteID) {

        // load the appropriate Parse.ly tracker for the selected site ID
        (function(d) {
            var b = d.body;
            var e = d.createElement('script');

            e.id = 'parsely-cfg';
            e.src = '//cdn.parsely.com/keys/' + parselySiteID + '/p.js';
            b.appendChild(e);
        })(document);
    }


</script>
