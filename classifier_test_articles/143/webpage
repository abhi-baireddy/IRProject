                                        advert.setTargeting('aamId', AamGpt.getCookie('aam_uuid'));
                                    }
                                    if (pxDataPhase && pxDataTarget) {
                                        advert.setTargeting('px_phase', pxDataPhase)
                                                .setTargeting('px_target', pxDataTarget);
                                    }
                                    
                                    //googletag.pubads().enableSingleRequest();
                                    
                                    googletag.pubads().disableInitialLoad();
                                    googletag.enableServices();
                                    googletag.display('div-gpt-ad-1587200364134-1x1');
                                    googletag.pubads().refresh([advert], {changeCorrelator: false});
                                    LazyAdManager.push({id: 'div-gpt-ad-1587200364134-1x1', yPos: -1, display: true, slot: advert});
                                    ad_slots.push(advert);
                                });
                            </script>
                        </div>
                    </div>
                    <div class="no-gap">
                        <div id='div-gpt-ad-1587200364134-oop'>
                            <script type='text/javascript'>
                                if(!('ad_slots' in window)){
                                    var ad_slots = [];
                                }
                                googletag.cmd.push(function () {
                                    var advert = googletag.defineOutOfPageSlot('/1125/bet.com/news', 'div-gpt-ad-1587200364134-oop').addService(googletag.pubads())
                                            .setTargeting('n', 'three-americans-killed-in-terror-attack-on-u-s--military-base-in')
                                            
                                                .setTargeting('tag','national_news')
                                            
                                            .setTargeting('sub', 'national')
                                            .setTargeting('prod', '')
                                            .setTargeting('show', '')
                                            .setTargeting('tile', '1')
                                            .setTargeting('loc', '1')
                                            .setTargeting('ct', 'articlepage')
                                            .setTargeting('betts', '1587200364134')
                                            .setTargeting('ord', timestampDart)
                                            .setTargeting('cat', 'news')
                                            
                                            
                                            
                                            ;
                                    function getUrlCidParameter() {
                                        var matches = document.URL.match(/(cid|CID)=([^&]+)/i);
                                        var cidParam;
                                        try{
                                            cidParam = matches && decodeURIComponent(matches[2]);
                                        } catch(e) {
                                            cidParam = matches && matches[2];
                                        }
                                        if (sessionStorage) {
                                            cidParam && sessionStorage.setItem("cid", cidParam);
                                            cidParam = sessionStorage.getItem("cid");
                                        }
                                        return cidParam;
                                    }
                                    var cidParameter = getUrlCidParameter();
                                    if(cidParameter != null ){
                                        advert.setTargeting('cid', cidParameter);
                                    }
                                    if (typeof AamGpt.getCookie('betgpt') != "undefined") {
                                        advert.setTargeting(AamGpt.getKey('betgpt'), AamGpt.getValues('betgpt'));
                                    }
                                    if (typeof AamGpt.getCookie('aam_uuid') != "undefined") {
                                        advert.setTargeting('aamId', AamGpt.getCookie('aam_uuid'));
                                    }
                                    if (pxDataPhase && pxDataTarget) {
                                        advert.setTargeting('px_phase', pxDataPhase)
                                                .setTargeting('px_target', pxDataTarget);
                                    }
                                    
                                    // googletag.pubads().enableSingleRequest();
                                    
                                    googletag.pubads().disableInitialLoad();
                                    googletag.enableServices();
									googletag.pubads().addEventListener('slotRenderEnded', function(event) {
										if (event.slot === advert) {
											if(!event.isEmpty){                                               
                                                var body_node = document.getElementsByTagName('body')[0];
                                                var added_nodes = [];
                                                var triggered_tracking = false;                                                
                                                var config = { attributes: true, childList: true };                                                
                                                var callback = function(mutationsList) {
                                                    var i = 0;
                                                    while(i < mutationsList.length) {
                                                        if (mutationsList[i].type == 'childList') {
                                                            if(mutationsList[i].addedNodes.length>0){
                                                                added_nodes = added_nodes.concat(mutationsList[i].addedNodes[0]);
                                                            }
                                                        }
                                                        else if (mutationsList[i].type == 'attributes') {                                                        
															if(!triggered_tracking){
																checkAddedNodesSize(added_nodes);
															}
                                                        }
														i++;
                                                    }
                                                };
                                                var checkAddedNodesSize = function(added_nodes){
                                                    var i = 0;
                                                    var full_height = false;
                                                    var full_width = false;                                          
                                                    while(i<added_nodes.length && !triggered_tracking){                                                        
                                                        if(parseInt(added_nodes[i].style["z-index"])>=2000000000){
                                                            triggered_tracking = true;
                                                        }
                                                        else if(added_nodes[i].className.indexOf('sprkflow_main')>-1){
                                                            triggered_tracking = true;
                                                        }
                                                        if(triggered_tracking){
															triggerTrackingForFullScreenPopUpAd(added_nodes[i]);
                                                            observer.disconnect();
                                                        }
                                                        i++;
                                                    } 
                                                }                                                
                                                var observer = new MutationObserver(callback);                                               
                                                observer.observe(body_node, config);                                                
                                                var triggerTrackingForFullScreenPopUpAd = function(ad_elem) {													
													window.s.linkTrackVars='events';
													window.s.linkTrackEvents='event155';													
													window.s.events='event155';
													window.s.tl(true,'o', 'Undertone Full Screen Ad');
													
											    }
											}
										}
									});
                                    googletag.display('div-gpt-ad-1587200364134-oop');
                                    googletag.pubads().refresh([advert], {changeCorrelator: false});
                                    LazyAdManager.push({id: 'div-gpt-ad-1587200364134-oop', yPos: -1, display: true, slot: advert});
                                    ad_slots.push(advert);
                                });
                            </script>
                        </div>
                    </div>
                
            
        
    
    



    
    


    </div>
    








