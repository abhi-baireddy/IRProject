                                            return cidParam;
                                        }
                                        var cidParameter = getUrlCidParameter();
                                        if(cidParameter != null ){
                                            advert.setTargeting('cid', cidParameter);
                                        }
                                    
                                } else {
                                    
                                        var advert = googletag.defineSlot('/1125/bet.com/news', [[300, 250],[300, 600]], 'div-gpt-ad-1587200364201')
                                            
                                            .addService(googletag.pubads())
                                            .setTargeting('n', 'three-americans-killed-in-terror-attack-on-u-s--military-base-in')
                                            
                                                .setTargeting('tag','national_news')
                                            
                                            .setTargeting('sub', 'national')
                                            .setTargeting('prod', '')
                                            .setTargeting('show', '')
                                            .setTargeting('tile', '2')
                                            .setTargeting('loc', '2')
                                            .setTargeting('ct', 'articlepage')
                                            .setTargeting('betts', '1587200364201')
                                            .setTargeting('ord', timestampDart)
                                            .setTargeting('cat', 'news')
                                            
                                            
                                            
                                            ;
                                        function getUrlCidParameter() {
                                            var matches = document.URL.match(/(cid|CID)=([^&]+)/i);
                                            var cidParam;
                                            try{
                                                cidParam = matches && decodeURIComponent(matches[2]);
                                            } catch(e) {
                                                cidParam = matches && matches[2];
                                            }
                                            if (sessionStorage) {
                                                cidParam && sessionStorage.setItem("cid", cidParam);
                                                cidParam = sessionStorage.getItem("cid");
                                            }
                                            return cidParam;
                                        }
                                        var cidParameter = getUrlCidParameter();
                                        if(cidParameter != null ){
                                            advert.setTargeting('cid', cidParameter);
                                        }
                                    
                                }
                                if (typeof AamGpt.getCookie('betgpt') != "undefined") {
                                    advert.setTargeting(AamGpt.getKey('betgpt'), AamGpt.getValues('betgpt'));
                                }
                                if (typeof AamGpt.getCookie('aam_uuid') != "undefined") {
                                    advert.setTargeting('aamId', AamGpt.getCookie('aam_uuid'));
                                }
                                if (pxDataPhase && pxDataTarget) {
                                    advert.setTargeting('px_phase', pxDataPhase)
                                            .setTargeting('px_target', pxDataTarget);
                                }
                                
                                //googletag.pubads().enableSingleRequest();
                                
                                googletag.pubads().disableInitialLoad();
                                googletag.enableServices();
                                googletag.display('div-gpt-ad-1587200364201');
                                
                                
                                LazyAdManager.push({id: 'div-gpt-ad-1587200364201', yPos: -1, display: false, slot: advert, });
                                
                                ad_slots.push(advert);
                            });
                }
                        </script>
