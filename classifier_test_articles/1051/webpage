<div id="trending_container" class="article-tile popular-box masonry-tile ">

  <div class="popular-box-wrapper" style="display: none;">
    <div class="popular-box-header">
      <img class="popular-box-image" src="https://static.pjmedia.com/static/2001394/images/pj-square.png">
      <div class="popular-box-header-text">Popular Now</div>
    </div>
    <div id="trending" class="popular-box-content"></div>
  </div>

