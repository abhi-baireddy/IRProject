var disqus_config = function () {
  this.page.remote_auth_s3 = '';
  this.page.api_key = '';
  this.page.url = 'https://pjmedia.com/trending/supreme-court-rules-trump-admin-can-detain-illegal-alien-criminals-at-any-time/';
  this.page.identifier = '51-108979';
  this.page.title = 'Supreme Court Rules Trump Admin. Can Detain Illegal Alien Criminals at Any Time';
  this.callbacks.onNewComment = [function(comment) {
    pjm.gabuddy.sendGAEvent('Comments', 'New Comment', 'Supreme Court Rules Trump Admin. Can Detain Illegal Alien Criminals at Any Time', 1);
  }];
};  

