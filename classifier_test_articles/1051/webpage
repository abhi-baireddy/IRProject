    } else {
        // Other pages
        function sticky_relocate() {
            var window_top = $( window ).scrollTop();
            var div_top = $('.sticky-wrapper').length ? $('.sticky-wrapper').offset().top + 500 : 0;
            var sidebar_height = $( '#article-sidebar' ).height();
            var sidebar_bottom = $( '#article-sidebar' ).offset().bottom;


            //if (window_top > div_top) {
            if (window_top > sidebar_height ) {
                var sidebar_height = $( '#article-sidebar' ).height();
