            oopAdModule = jQuery('#out-of-page-ad-module'),
            signupFormId = '#out-of-page-signup-nl-form';

        //replaces creatives TW_LLLU_Desktop_NL_Signup_403x320 and TW_LLLU_Mobile_NL_Signup_300x150 content 
        //with newsletter signup form
        googletag.pubads().addEventListener('slotRenderEnded', function(event) {
            var slotId = event.slot.getSlotElementId();

            //if not mobile device change signup source to desktop
            if (!isMobile) { jQuery(signupFormId).find('input[name="signupsource"]').attr("value", "out_of_page_ad_desktop"); }

            if (slotId.indexOf('div-gpt-ad-14229977701') !== -1) {
                if (event.creativeId == '138292601797'||event.creativeId == '138293042337') {
                    //console.log('newsletter creative found', event.creativeId, 'slot id', event.slot.getSlotElementId());
                    var hideModule = function(){
                            if (oopAdModule.hasClass('slide-up')) {
                                oopAdModule.addClass('slide-down').removeClass('slide-up');
                            }
                        },
                        autoHideModule = setTimeout(hideModule, 20000),
                        hideModuleOnSuccess = function() { setTimeout(hideModule, 3000); };

                    oopAdModule.prepend(signupModule);
                    validateSubmitSignupNLForm(jQuery(signupFormId), {}, hideModuleOnSuccess);
                    oopAdModule.delay(2000).addClass('slide-up');
                    
                    //clear timeout if user is entering email
                    jQuery('#out-of-page-signup-nl-form input.email-input').focus(function() {
                        clearTimeout(autoHideModule);
                        jQuery(this).off('focus');//removes listener
                    });
                }
            }
        });
    });

