	<script>
	var newsletterTicked3;
    jQuery(document).ready(function() {
		articleId = window.location.pathname.split( '/' )[2];
		jQuery("#newsletter-form").validate({
			rules: {
				email: {
					required: true
				   ,email: true
				}
			},
			errorPlacement: function(error, element) {
				error.appendTo( "#newsletter-error" );
			},
			submitHandler: function(form) {
				jQuery("#newsletter-form.send-btn").attr('disabled', 'disabled').css('opacity','.4');
				jQuery.ajax({
					url: '/signup',
					data: {email: jQuery("#newsletter-form>#email").val(), briefing:"true", top_opinions:"false", captured:"false", signupsource: "five_things_list_page"},
					dataType: 'json',
					jsonp: false,
					success: function(data) {
						console.log(data);
						if(data.success) {
							parent.formSubmitted = true;
							jQuery("#newsletter-form").hide();
							jQuery("#newsletter-success").css('display', 'block');
						}else if(data.resubscribe) {
							alert("Thank you for subscribing. You will be sent a confirmation email soon.");
						}else if(data.invalid_email) {
							alert("This is an invalid email address. Please enter a valid one.");
						}
						else {
							jQuery("#newsletter-form.send-btn").attr('disabled', false).css('opacity', '1');
							alert(data.message);
						}
					}
				});
			}
		});
	});
    </script>
			<script> var isDetail = true;
			var isMobile = TheWeek.getCurrentDevice() == 'mobile',
				isTablet = TheWeek.getCurrentDevice() == 'tablet',
				isDesktop = TheWeek.getCurrentDevice() == 'desktop';
			var offsetScroll = isMobile ? 55 : 100;
			var titleDetail ='/5things/835652/trump-vetoes-resolution-end-involvement-yemen-war';

			var articleId = window.location.href.toString().split('/')[4];
			if(jQuery('#'+articleId).length && window.location.hash == ''){
				  jQuery(window).scrollTop(jQuery('#'+articleId).offset().top - offsetScroll );
			}
		</script>
	
