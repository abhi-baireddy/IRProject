<script>
var pgPxInit = function(){
	if (window.requestIdleCallback) {
	    requestIdleCallback(function () {
	      pgDrawPixel(window.location.href);;
	    })
	} else {
	    setTimeout(function () {
	        pgDrawPixel(window.location.href);;
	    }, 500)
	}
}
if (typeof PGVARS !== "undefined") {
	if (PGVARS.requesttype === "STORY") {
		_PG._listen("storyloaded",function(){
			setTimeout(function(){
				pgPxInit();
			}, 1000);
		});
	} else {
		$(document).ready( function() {
			pgPxInit();
		});
	}
}
</script>



