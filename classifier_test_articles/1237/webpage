    if(!is_prime && !is_adfree && !is_inside) {document.querySelector('.jph_0').style.display="block";
    }
  });
</script>

<style>
  .jph_0 {
    display:none;
    color: #111;
    font-weight: normal;
    font-size: 17px;
    background: #f2f2f2;
