;(function($) {

  var _trending_container = $( '#trending_container' );
  var _trending = $( '#trending', _trending );

  // Populate Trending box
  var itemTemplate = $( 'script[data-template="sidebar-list-item"]' ).text().split( /\$\{(.+?)\}/g );

  if( _trending_container.length ) {

      var _moment = moment().format( 'YYMMDD-HHmm' ); // Bust cache every minute
      $.get( 'https://hotair.com/api/trending/pjmedia.json?ts=' + _moment, function( _items ) {

          _trending.append( _items.map( function( item ) {
              return itemTemplate.map( renderTemplateItem( item ) ).join( '' );
          }));

          if( _items.length ) $( 'div.popular-box-wrapper', _trending_container ).slideDown();

      });

  }

})( window.jQuery );
