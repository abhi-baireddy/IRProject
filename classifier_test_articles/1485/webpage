<script async type='text/javascript'>
;(function($, undefined) {

    var scrolltop=362; // Nav with title in it
    var minimumPosition=332; // Minimum point to scroll to before carousel is modified
    var enableCarousel=false;
    var showPosition=432;
    var currentPosition=$(document).scrollTop();
    var lastScrollTop = 0;

    $(window).scroll(function () {
      currentPosition=$(document).scrollTop();
      if (currentPosition > minimumPosition) {
        enableCarousel=true; // Enable when scroll point is reached
      }

      var st = $(this).scrollTop();
       if (st > lastScrollTop) {
            // Downscroll logic
       } else {
            // Upscroll logic
            if(st > lastScrollTop-50 || st < 362){
                enableCarousel=false;
            }
       }
       lastScrollTop = st;

      if (enableCarousel) {
        if (currentPosition < showPosition) {
          // console.log("current position: " + currentPosition + " show position: " + showPosition);
          if (!$(".thm-carousel").is(":visible")) {
            $(".thm-carousel").show();
            $('.owl-carousel').owlCarousel( {
              onRefresh: blazyRevalidate,  
              onChanged: blazyRevalidate,  
              autoWidth: true, 
              autoHeight: true, 
              stagePadding: 10, 
              loop: true, 
              lazyLoad: false, 
              dots: false, 
              margin: 10, 
              items: 15,
              nav: true, navContainerClass: "carousel-nav", navText: ['<a class="btn btn-default left carousel-control" role="button" data-slide="prev"><span class="icon-fa-chev-left"></span></a>', '<a class="btn btn-default right carousel-control" role="button" data-slide="next"><span class="icon-fa-chev-right"></span></a>']
            }
            )
          }
        }
        else {
          if ($(".thm-carousel").is(":visible")) {
            $(".thm-carousel").hide();
          }
        }
      }
      showPosition=currentPosition;
    }
    );

})( window.jQuery );

    function blazyRevalidate() {

        var bLazy = new Blazy( { selector: ".b-lazy" } );
        bLazy.revalidate();

    };

</script>


<script async type='text/javascript'>
;(function($, undefined) {

    var scrolltop=362; // Nav with title in it
    var minimumPosition=332; // Minimum point to scroll to before carousel is modified
    var enableCarousel=false;
    var showPosition=432;
    var currentPosition=$(document).scrollTop();
    var lastScrollTop = 0;

    $(window).scroll(function () {
      currentPosition=$(document).scrollTop();
      if (currentPosition > minimumPosition) {
        enableCarousel=true; // Enable when scroll point is reached
      }

      var st = $(this).scrollTop();
       if (st > lastScrollTop) {
            // Downscroll logic
       } else {
            // Upscroll logic
            if(st > lastScrollTop-50 || st < 362){
                enableCarousel=false;
            }
       }
       lastScrollTop = st;

      if (enableCarousel) {
        if (currentPosition < showPosition) {
          // console.log("current position: " + currentPosition + " show position: " + showPosition);
          if (!$(".thm-carousel").is(":visible")) {
            $(".thm-carousel").show();
            $('.owl-carousel').owlCarousel( {
              onRefresh: blazyRevalidate,  
              onChanged: blazyRevalidate,  
              autoWidth: true, 
              autoHeight: true, 
              stagePadding: 10, 
              loop: true, 
              lazyLoad: false, 
              dots: false, 
              margin: 10, 
              items: 15,
              nav: true, navContainerClass: "carousel-nav", navText: ['<a class="btn btn-default left carousel-control" role="button" data-slide="prev"><span class="icon-fa-chev-left"></span></a>', '<a class="btn btn-default right carousel-control" role="button" data-slide="next"><span class="icon-fa-chev-right"></span></a>']
            }
            )
          }
        }
        else {
          if ($(".thm-carousel").is(":visible")) {
            $(".thm-carousel").hide();
          }
        }
      }
      showPosition=currentPosition;
    }
    );

})( window.jQuery );

    function blazyRevalidate() {

        var bLazy = new Blazy( { selector: ".b-lazy" } );
        bLazy.revalidate();

    };

</script>

<script type="text/javascript" defer>

        (function($) {

            $(window).scroll(function(){

                var window_top = $(window).scrollTop();
                var _viewHeight = $(window).height();
                var div_top1 = $('.sticky-separator-1').offset().top - 10;
                var div_top2 = $('.sticky-separator-2').offset().top - 70;
                var div_top3 = $('.sticky-separator-3').offset().top - 70;

                (window_top > div_top1) ?
                    $('#sticky1').addClass('stick') : $('#sticky1').removeClass('stick');
                (window_top > div_top2) ?
                    $('#sticky2').addClass('stick') : $('#sticky2').removeClass('stick');
                (window_top > div_top3) ?
                    $('#sticky3').addClass('stick') : $('#sticky3').removeClass('stick');

                var _footerTop1 = $('.sticky-separator-2').offset().top + 20;
                var _footerTop2 = $('.sticky-separator-3').offset().top + 20;
                var _footerTop3 = $('#footer-wrapper').offset().top - 300;
                var _windowHeight = window_top + _viewHeight;
                var _sideHeight = $(this).outerHeight();
                var _footerVisible1 = _windowHeight - _footerTop1;
                var _footerVisible2 = _windowHeight - _footerTop2;
                var _footerVisible3 = _windowHeight - _footerTop3;

                (_viewHeight - (_footerVisible1 + _sideHeight) < 10) ?
                    $('#sticky1').css('margin-top', (_viewHeight - (_footerVisible1 + _sideHeight)  ) + 'px') : $('#sticky1').css('margin-top', '100px');
                (_viewHeight - (_footerVisible2 + _sideHeight) < 10) ?
                    $('#sticky2').css('margin-top', (_viewHeight - (_footerVisible2 + _sideHeight)   ) + 'px') : $('#sticky2').css('margin-top', '50px');
                (_viewHeight - (_footerVisible3 + _sideHeight) < 10) ?
                    $('#sticky3').css('margin-top', (_viewHeight - (_footerVisible3 + _sideHeight)  + 20 ) + 'px') : $('#sticky3').css('margin-top', '50px');

            });

       })(jQuery);

    </script>

