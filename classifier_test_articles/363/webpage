<p     
  
                  
  
  
 class="editorial-full__tag tag"
>
      <span class="tag__icon">
        <svg viewbox="0 0 24 24" class="icon icon--study"  aria-labelledby="icon-label--study" >
          <title id="icon-label--study">Research/Study</title>
    
    <use xlink:href="#study"></use>
