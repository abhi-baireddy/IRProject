window.DARLA_CONFIG.onFinishParse = function(eventName, response) {try {window._perfMark('DARLA_PEND');if (eventName === "prefetch") {window._DarlaPrefetchResponse = response;}if (window._DarlaEvents) {window._DarlaEvents.emit("finishparse", {response: response,eventName: eventName});}} catch (e) {console.error(e);throw e;}if (eventName === "AUTO") {var adPositionList = response.ps();var groupAdsList = ["LREC3","LREC4","MON2"];for (var i = 0; i < groupAdsList.length; i++) {if (adPositionList.indexOf(groupAdsList[i]) >= 0) {var adPosItem = response.item(groupAdsList[i]);if (!adPosItem.hasErr && adPosItem.size + "" !== "1x1" && (adPosItem.meta && adPosItem.meta.y && adPosItem.meta.y.size && adPosItem.meta.y.size + "" !== "1x1")) {var adNode = document.getElementById("default" + groupAdsList[i] + "-sizer");if (adNode && adNode.classList) {adNode.classList.remove("D(n)");adNode.classList.remove("D-n");}}}}}
