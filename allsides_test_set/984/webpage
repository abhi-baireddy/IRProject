    <div class="column" id="coronavirusform">    
    	<form id="coronavirus-signup-form" method="post", action="/emailprefs/list-add?list=Coronavirus+Bulletin">
		<div class="row">
    	<div class="column left">
			<input required type="email" name="email" placeholder="Email address" /> 
   		</div>
