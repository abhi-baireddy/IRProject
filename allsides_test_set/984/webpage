<script>
        /* global $ */

        var usnSignupHook = function() {
            if (!window.jQuery) {
            	window.setTimeout(usnSignupHook, 200);
                return;
        	}
            
            var $form = $('#coronavirus-signup-form');

            if (window.fetch) {
                var $successMessage = $('<p id="coronavirus-signup-success" style="padding: 0; margin: 1rem 0 0 0;">Thank you, your email address has been submitted.</p>');

                $form.on('submit', function(e) {
                    var email = $form.find('input[name=email]').val();

                    e.preventDefault();
                    $form.find('#coronavirus-signup-success').remove();

                    fetch($form.attr('action') + '&email=' + email, {
                        method: 'POST',
                        body: JSON.stringify({ source: 'coronavirus-module' }),
                        headers: { 'Content-Type': 'application/json' }
                    })
                        .then(res => res.json())
                        .then(res => {
                            $form.append($successMessage);
                        })
                        .catch(e => {
                            $form.find('#coronavirus-signup-success').remove();
                        });
                });
            }
        };
    
        usnSignupHook();
    </script> 
</div>
