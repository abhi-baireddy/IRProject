        if (true &amp;&amp; window.apstag &amp;&amp; true &amp;&amp; window.headertag) {
          let params = {
            id: &#039;ent_movies_1841&#039;,
            slotName: &#039;/4011/trb.chicagotribune/ent/movies&#039;,
            includeInA9: true,
