  ;(function() {
    let previousBreakpoint = null;
    let appBarMutationObAdded;
    const floatingChainWrapper = document.querySelector(".floating-chain-wrapper-1514");
    const closeTrayEl = document.querySelector('div[data-role="close-tray"]');

    function initFloatingChain() {
      const viewportWidth = window.innerWidth || document.documentElement.clientWidth || document.body.clientWidth;

      // Only set "floating" functionality on desktop
      if (viewportWidth >= 800 && checkBreakpointChange('desktop')) {
        setFloatingChainStyles(true);
      } else if (viewportWidth < 800 && checkBreakpointChange('mobile')) {
        setFloatingChainStyles(false);
      }
    }
    
    function handleAppBarMutations() {
      appBarMutationObAdded = true
      const appBar = document.getElementById('app-bar-sticky');
      if (appBar) {
        let appBarHeight;
        let observer = new MutationObserver(function(mutations) {
          mutations.forEach(function(mutation) {
            let currentAppBarHeight = appBar.clientHeight
            if (currentAppBarHeight !== appBarHeight) {
              floatingChainWrapper.style.top = currentAppBarHeight+'px'
              appBarHeight = currentAppBarHeight
            }
          });
        });
        observer.observe(appBar, { attributes: true, childList: true });
      }
    }

    function checkBreakpointChange(currentBreakpoint) {
      const isBreakpointChanged = !previousBreakpoint || previousBreakpoint !== currentBreakpoint;
      previousBreakpoint = currentBreakpoint;
      return isBreakpointChanged;
    }

    function setFloatingChainStyles(setToSticky) {
      const classListAction = setToSticky ? 'add' : 'remove';
      if (closeTrayEl) {
          closeTrayEl.classList[classListAction]('overflow-x-inherit');
      }
      floatingChainWrapper.classList[classListAction]('sticky');
      floatingChainWrapper.classList[classListAction]('z-index-29');
      
      // Add mutation observer to check if the height of appbar changes
      setToSticky ? !appBarMutationObAdded && handleAppBarMutations() : floatingChainWrapper.style.top = '0'
    }

    initFloatingChain();
    window.addEventListener('resize', initFloatingChain);
  })();
</script> <div class="pb-container"> </div> </section> </main> <div class="artcl--r-sp"> <aside id="left-bottom" class="artcl--sect-tmpl left guttered"> <div class="wrapper clearfix full pb-feature pb-layout-item pb-f-global-vertical-spacer" data-pb-name="Vertical Spacer" data-pb-curated="curated" data-pb-id="unnamed-feature" id="f0tA2GBsZNDQPr"> <div class="hddn-mble hddn-tbl" style="height:0px"></div> </div> <div class="wrapper clearfix full pb-feature pb-layout-item pb-f-list-recommender" data-pb-name="Recommender" data-pb-curated="automated" data-pb-id="bottom-of-listicle-recommender" id="fSyhKF1sZNDQPr"> <div class="crd rcom--ctn flex-container-column align-items-stretch "> <div class="crd--cnt "> <ul class="list-unstyled clln clln-vtl clln-crd "> <li class="clln--it clln-it-bdr-btm "> <p class="h5 capitalized spaced spaced-bottom spaced-sm">Recommended</p> </li> <li class="clln--it flex-container-row align-items-start rcom--it clln-it-bdr-btm "> <a href="https://www.chicagotribune.com/visuals/ct-viz-coronavirus-chicago-photos-20200304-ncsg6xp5jjcvpccofmxjeolyu4-photogallery.html#nt=latestnews&rt=subtype-lists" tabindex="-1"> <figure class="" data-trk-photo-credit=""> <div class="lf-el image-xs img-container aspect-ratio-1x1"> <img class="b-lazy full-width" alt="Coronavirus in Chicago: A look at how people are responding and preparing" data-src="https://www.chicagotribune.com/resizer/GYDeapUU5wuEO35u5MW9ZF-JtTU=/72x72/top/arc-anglerfish-arc2-prod-tronc.s3.amazonaws.com/public/AZ4VZTIZ5FFITD6C25FBTF7EWQ.jpg" data-src-small="" data-src-medium="" data-src-mobile="" src="/pb/resources/images/tinygif.gif"/> </div> </figure> </a> <div class="flex-container-column flex"> <div class="recommender"> <a href="https://www.chicagotribune.com/visuals/ct-viz-coronavirus-chicago-photos-20200304-ncsg6xp5jjcvpccofmxjeolyu4-photogallery.html#nt=latestnews&rt=subtype-lists"> <p class="h6"> Coronavirus in Chicago: A look at how people are responding and preparing </p> </a> </div> <