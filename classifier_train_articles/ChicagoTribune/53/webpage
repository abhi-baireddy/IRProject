              oVa["trb_ad_970x250/970x90/728x90/320x100/320x50_1_/sports/bears/ct-cb-chicago-bears-nfl-mock-draft-lists"] = ["NULL"];
            } else {
              oPageUnload('trb_ad_970x250/970x90/728x90/320x100/320x50_1_/sports/bears/ct-cb-chicago-bears-nfl-mock-draft-lists', true);
            }
          }
        
      
      // Validation to fix ORCA-896 issue
      
        function loadAd() {
            dfpService.appendAd({
                id: 'sports_football_bears_sf_1509',
                optimeraId: 'trb_ad_970x250/970x90/728x90/320x100/320x50_1_/sports/bears/ct-cb-chicago-bears-nfl-mock-draft-lists',
