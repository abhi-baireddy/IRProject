     <script>
     $(function matherTrack() {
         ((window.trb || (trb = {})).registration || (trb.registration = {})).push(function(reg) {
           reg.utils.poll('a[href].btn', function() {
             var pids = [],
                 offers = [],
                 dssP = 'baltngux',
                 delivZip = '21201',
                 nonDelivZip = '90012';
             getPids();
             buildOffers(delivZip, nonDelivZip);
             (window._matherq = window._matherq || []).push(["paywallEvent", {category: "payment", action: "subscriptions_offered", offers: offers}]);
   
             function getPids() {
                 $('a[href].btn').each(function(k, v) {
                     var qu = v.href.match(/[\\?&]pid=([^&#]*)/i),
                         pid = qu && qu[1];
                     pid && pids.push(pid);
                 });
             }
   
             function buildOffers(delivZip, nonDelivZip) {
                 var offerHash = {};
                 getOffers(delivZip);
                 getOffers(nonDelivZip);
                 $.each(offerHash, function(k, v) {
                     offers.push(v)
                 });
   
                 function getOffers(zip) {
                     $.ajax({
                         url: "https://www.tribdss.com/subscriptions/offerspanel/" + dssP + '/' + zip + "?pid=" + pids.join(","),
                         async: false,
                         success: function(result) {
                             var offersResp = result.offers;
                             $(offersResp).each(function(k, v) {
                                 var offerId = v.plan_id + '|' + v.promotion_id;
                                 !offerHash[offerId] && (offerHash[offerId] = {
                                     'offerId': offerId,
                                     'offerName': v.offer_name,
                                     'promotionId': v.promotion_id,
                                     'price': v.pricing_term,
                                     'per': 'month',
                                     'termAmount': '1',
                                     'termUnit': 'month'
                                 });
                             });
                         }
                     });
                 }
   
             }
           });
         });
     });
     </script>
