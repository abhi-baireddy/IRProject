                        <script type="IN/Share" data-counter="top"></script>
                    </div>
                </li>
            </ul>
            <ul id="sharebarx">
                <li><div id="artPgSocHorFB"></div></li>
                <li><div id="artPgSocHorTw"></div></li>
                <li><div id="artPgSocHorGo"><g:plusone size="medium"></g:plusone></div></li>
                <li>
                    <div id="artPgSocHorLi">
                        <script src="https://platform.linkedin.com/in.js" type="text/javascript"></script>
                        <script type="IN/Share" data-counter="right"></script>
