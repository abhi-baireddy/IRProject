    <script type="text/javascript">

        jQuery(document).ready(function () {
              
            jQuery("#txtEmail").focusin(function () {
                    //analyticsFormTracking('84', "form_start");
            });
             
            
        });

        function GetCheckButtonValue(chktbl) {
            var checked = false;
            try {
                var checkboxs = chktbl.getElementsByTagName('input');
                for (var j = 0; j < checkboxs.length; j++) {
                    if (checkboxs[j].type === 'checkbox') {
                        if (jQuery('#' + checkboxs[j].id).is(':checked')) {
                            checked = true;
                            break;
                        }
                    }
                }
            }
            catch (e) {
            }
            return checked;
        }

        function GetRadioButtonValue(radiotbl) {

            var checked = false;
            try {
                var radios = radiotbl.getElementsByTagName('input');
                for (var j = 0; j < radios.length; j++) {
                    if (radios[j].type == 'radio') {
                        if (radios[j].checked == true) {
                            checked = true;
                        }
                    }
                }
            }
            catch (e) {
            }
            return checked;
        }

        // Detect Enter Key
        document.onkeypress = function (e) {
            e = e || window.event;
            var charCode = (typeof e.which == "number") ? e.which : e.keyCode;
            if (charCode === 13) {
                validate();
            }
        };

        function validate() {
            var answeredall = true;
            var answeredallchk = true;
            var qdiv = document.getElementById('qcontainer');
            var qdivs = qdiv.getElementsByTagName('input');
            for (var i = 0; i < qdivs.length; i++) {
                if (qdivs[i].id.indexOf('hfIsRequired', 0) > 1) {
                    if (qdivs[i].value == 'True') {
                        var radiotbl = document.getElementById(qdivs[i].id.substring(0, qdivs[i].id.indexOf('hfIsRequired', 0)) + 'rblOptions');
                        if (radiotbl != null) {
                            if (!GetRadioButtonValue(radiotbl)) {
                                answeredall = false;
                                var span = document.getElementById(qdivs[i].id.substring(0, qdivs[i].id.indexOf('hfIsRequired', 0)) + 'spIsRequired');
                                span.style.visibility = 'visible';
                                span.style.display = 'inline';
                                //span.innerText = '*';
                            }
                            else {
                                var span = document.getElementById(qdivs[i].id.substring(0, qdivs[i].id.indexOf('hfIsRequired', 0)) + 'spIsRequired');
                                span.style.visibility = 'hidden';
                                span.style.display = 'none';
                                //span.innerText = '';
                            }
                        }
                        var chktbl = document.getElementById(qdivs[i].id.substring(0, qdivs[i].id.indexOf('hfIsRequired', 0)) + 'cblOptions');
                        if (chktbl != null) {
                            if (!GetCheckButtonValue(chktbl)) {
                                answeredallchk = false;
                                var span = document.getElementById(qdivs[i].id.substring(0, qdivs[i].id.indexOf('hfIsRequired', 0)) + 'spIsRequired');
                                span.style.visibility = 'visible';
                                span.style.display = 'inline';
                            }
                            else {
                                var span = document.getElementById(qdivs[i].id.substring(0, qdivs[i].id.indexOf('hfIsRequired', 0)) + 'spIsRequired');
                                span.style.visibility = 'hidden';
                                span.style.display = 'none';
                            }
                        }
                    }
                }
            }
            if (answeredall && answeredallchk) {
                if (submitUserForm()) {
                    var btnsave = document.getElementById('p_lt_zoneContent_pageplaceholder_p_lt_zoneTop_SurveyForm_btnSave');
                    btnsave.click();
                }
            }
            else {
                //Scroll to first error
                jQuery("html, body").animate({ scrollTop: ((jQuery(".surveyError:visible:first").offset().top) - 20) });
            }
            return false;
        }
        function submitUserForm() {
            var response = grecaptcha.getResponse();
            if (response.length == 0) {
                document.getElementById('g-recaptcha-error').innerHTML = '<span style="color:red;">This field is required.</span>';
                return false;
            }
            return true;
        }

        function verifyCaptcha() {
            document.getElementById('g-recaptcha-error').innerHTML = '';
        }

    </script>

</div>
</br><br />
<div style="margin-left: 10px;clear:both;"><span class="surveyRed"><strong>Please Note:</strong></span>  In appreciation of casting your vote, Newsmax will keep you automatically updated on poll results and other breaking news with <strong>FREE</strong> daily email alerts. If you wish to discontinue these updates, you are welcome to unsubscribe at any time. Invalid email addresses will not be counted in Newsmax polls. Your email address and personal information is confidential as stated in our <a href="/PrivacyStatement" style="text-decoration: underline;" target="_blank">Privacy Policy</a>. Thank you.</div>






</div>

