        <script type="text/javascript">


            function readCookie(name) {
                var nameEQ = name + "=";
                var ca = document.cookie.split(';');
                for (var i = 0; i < ca.length; i++) {
                    var c = ca[i];
                    while (c.charAt(0) === ' ') c = c.substring(1, c.length);
                    if (c.indexOf(nameEQ) === 0) return c.substring(nameEQ.length, c.length);
                }
                return null;
            }

            var login_from_vuukle = function () {
                window.location.href = OAuthClient.getLoginURI();
            }

            var VUUKLE_CONFIG = {
                apiKey: '3658ec56-b1fa-407e-9a1e-0c2c5ecd394a',
                articleId: '959081',
                img: 'https://www.newsmax.com/CMSPages/GetFile.aspx?guid=44ba08fc-db6d-479e-b71a-f787c17e5d79&SiteName=Newsmax&maxsidesize=600',
                link: 'https://www.[url]',
                host: 'newsmax.com',
                ads: { enabled: false },
                comments: {
                    maxChars: 20000,
                    countToLoad: '2',
                    hideCommentInputBoxWithButton: false,
                    toxicityLimit: '0', // disabled
                    spamLimit: '0', // disabled
                    editorOptions: ['italic', 'underline'],
                    auth: {
                        facebook: false,
                        google: false,
                        twitter: false,
                        disqus: false,
                        password: false,
                        vuukle: false,
                        sso: {
                            onClick: login_from_vuukle
                        }
                    }
                }
            };


            function loadVuuklePlatform () {
                var d = document,
                    s = d.createElement('script');
                s.src = 'https://cdn.vuukle.com/platform.js';
                (d.head || d.body).appendChild(s);

                if (typeof window.vuukleLogin !== "undefined" && readCookie('sso') === null) {
                    window.vuukleLogin();
                }
            }

            function renderVuukle() {
                document.getElementById("showVuukleLink").style.display = "none";
                document.getElementById("contentContainer").style.display = "block";

                loadVuuklePlatform();
            }
        </script>

