    // used for showing and hiding bio
    function ShowFullBio() {
        if ('Hide Full Bio' == jQuery('#aFullBio').html().trim()) {
            jQuery('#aFullBio').html('Show Full Bio');
            jQuery('#nmhExpandedBioSection').addClass('HideBio');
            jQuery('#nmhExpandedBioSection').removeClass('ShowBio');

        }
        else {
            jQuery('#aFullBio').html('Hide Full Bio');
            jQuery('#nmhExpandedBioSection').removeClass('HideBio');
            jQuery('#nmhExpandedBioSection').addClass('ShowBio');
        }

    }

    </script>

    
  

