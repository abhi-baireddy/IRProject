                    countToLoad: '2',
                    hideCommentInputBoxWithButton: false,
                    toxicityLimit: '0', // disabled
                    spamLimit: '0', // disabled
                    editorOptions: ['italic', 'underline'],
                    auth: {
                        facebook: false,
                        google: false,
                        twitter: false,
                        disqus: false,
                        password: false,
                        vuukle: false,
                        sso: {
                            onClick: login_from_vuukle
                        }
                    }
                }
            };


            function loadVuuklePlatform () {
                var d = document,
                    s = d.createElement('script');
                s.src = 'https://cdn.vuukle.com/platform.js';
                (d.head || d.body).appendChild(s);

                if (typeof window.vuukleLogin !== "undefined" && readCookie('sso') === null) {
                    window.vuukleLogin();
                }
            }

            function renderVuukle() {
                document.getElementById("showVuukleLink").style.display = "none";
                document.getElementById("contentContainer").style.display = "block";

                loadVuuklePlatform();
            }
        </script>

        <span id="p_lt_zoneContent_pageplaceholder_p_lt_zoneLeft_VuukleThread_lblAccessToken"></span>
        <div id="ssostat" style="display: none"></div>
    </div>
    <script type="text/javascript" src="/js/sso/oauthclient.js?v=1.5"></script>

</div>
<script type="text/javascript">
jQuery(document).ready(function(){
	jQuery('.CommentRef').bind('click', function(){
		jQuery('html,body').animate({scrollTop: jQuery('#CommentSection').offset().top},'slow');
	});
});
    function scrollToAnchor(anchorId){
    var aTag = jQuery(anchorId);
    $jQuery('html,body').animate({scrollTop: aTag.offset().top},'slow');
}
</script>
