  <source srcset="https://cdn.vox-cdn.com/thumbor/lYR52lAAwpEye_DNxJ40aAhPovs=/0x0:1941x1941/320x0/filters:focal(0x0:1941x1941):format(webp):no_upscale()/cdn.vox-cdn.com/uploads/chorus_asset/file/19833653/flu_covid_comparison_1_high_res__1_.jpg 320w, https://cdn.vox-cdn.com/thumbor/tRFHkeJc9iOOZE9L5UJZTz7-dv8=/0x0:1941x1941/520x0/filters:focal(0x0:1941x1941):format(webp):no_upscale()/cdn.vox-cdn.com/uploads/chorus_asset/file/19833653/flu_covid_comparison_1_high_res__1_.jpg 520w, https://cdn.vox-cdn.com/thumbor/WkpzEkEfap5pHkOhEEwybP-uKDc=/0x0:1941x1941/720x0/filters:focal(0x0:1941x1941):format(webp):no_upscale()/cdn.vox-cdn.com/uploads/chorus_asset/file/19833653/flu_covid_comparison_1_high_res__1_.jpg 720w, https://cdn.vox-cdn.com/thumbor/bIMycLdpzNClzE_vtDZH2MNVIxE=/0x0:1941x1941/920x0/filters:focal(0x0:1941x1941):format(webp):no_upscale()/cdn.vox-cdn.com/uploads/chorus_asset/file/19833653/flu_covid_comparison_1_high_res__1_.jpg 920w, https://cdn.vox-cdn.com/thumbor/kf4YX_wBVC9fYdoQEYRml56Qc-s=/0x0:1941x1941/1120x0/filters:focal(0x0:1941x1941):format(webp):no_upscale()/cdn.vox-cdn.com/uploads/chorus_asset/file/19833653/flu_covid_comparison_1_high_res__1_.jpg 1120w, https://cdn.vox-cdn.com/thumbor/9jCocP6nuePU3ps0X_zRMtUTPjc=/0x0:1941x1941/1320x0/filters:focal(0x0:1941x1941):format(webp):no_upscale()/cdn.vox-cdn.com/uploads/chorus_asset/file/19833653/flu_covid_comparison_1_high_res__1_.jpg 1320w, https://cdn.vox-cdn.com/thumbor/hJduWVglfib1vcugTmYD8mdAo3Y=/0x0:1941x1941/1520x0/filters:focal(0x0:1941x1941):format(webp):no_upscale()/cdn.vox-cdn.com/uploads/chorus_asset/file/19833653/flu_covid_comparison_1_high_res__1_.jpg 1520w, https://cdn.vox-cdn.com/thumbor/EK0fCJgGre8lg9tkPahVzO1U2r8=/0x0:1941x1941/1720x0/filters:focal(0x0:1941x1941):format(webp):no_upscale()/cdn.vox-cdn.com/uploads/chorus_asset/file/19833653/flu_covid_comparison_1_high_res__1_.jpg 1720w, https://cdn.vox-cdn.com/thumbor/W2JsWODraobjtmnLZcwABax0BuI=/0x0:1941x1941/1920x0/filters:focal(0x0:1941x1941)