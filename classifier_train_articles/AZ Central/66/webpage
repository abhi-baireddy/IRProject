     data-cst="travel" data-base-page-type="story"
     data-published-date="2015-07-14T18:09:30.857Z"
     
     
      data-gal-pageurl="https://www.azcentral.com/picture-gallery/travel/2015/07/13/disneyland-the-early-years/30090567/"><div class="pag-viewport-thumb-wrapper story-asset"><div class="viewport gallery-viewport pag-viewport"><div class="slide gallery-viewport-slide  active"  itemprop="primaryImageOfPage"  itemscope itemtype="http://schema.org/ImageObject"><div class="pag-photo-border gallery-photo-border"><img itemprop="image" class="gallery-photo " data-byline="" data-mycapture-src="" data-mycapture-sm-src=""
                                    
                                        
