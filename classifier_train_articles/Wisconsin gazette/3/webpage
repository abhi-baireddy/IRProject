<script>
    $('#site-search-535923-btn').on(__tnt.client.clickEvent, function(){
        $('#site-search-535923').submit();
        return false;
    });

    $('#site-search-535923').on('submit', function(){
        // Filter query
        var sQueryFiltered = $("#site-search-535923 input[name=q]").val().replace(/\?/g, '');
    $("#site-search-535923 input[name=q]").val( sQueryFiltered );

        if( $('#site-search-535923-term').val() ){
            return true;
        }else{
            return false;
        }
    });</script>
