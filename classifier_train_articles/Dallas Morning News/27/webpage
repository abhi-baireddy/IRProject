      /* Cookie validation logic */
      var isLoggedIn = function isLoggedIn(cookie) {
        if (typeof cookie !== "string") {
          return false;
        }

        return cookie && cookie.indexOf("^") !== 0 && cookie.lastIndexOf("^") !== cookie.length - 1;
      };
      /* Copied from cookie NPM package */


      function tryDecode(str, decode) {
        try {
          return decode(str);
        } catch (e) {
          return str;
        }
      }
      /* Copied from cookie NPM package */


      var parseCookies = function parseCookies(str, options) {
        if (typeof str !== "string") {
          throw new TypeError("argument str must be a string");
        }

        var obj = {};
        var opt = options || {};
        var pairSplitRegExp = /; */;
        var pairs = str.split(pairSplitRegExp);
        var dec = opt.decode || decodeURIComponent;

        for (var i = 0; i < pairs.length; i++) {
          var pair = pairs[i];
          var eq_idx = pair.indexOf("=");
          /* skip things that don't look like key=value */

          if (eq_idx < 0) {
            continue;
          }

          var key = pair.substr(0, eq_idx).trim();
          var val = pair.substr(++eq_idx, pair.length).trim();
          /* quoted values */

          if ('"' == val[0]) {
            val = val.slice(1, -1);
          }
          /* only assign once */


          if (undefined == obj[key]) {
            obj[key] = tryDecode(val, dec);
          }
        }

        return obj;
      };
      /* Copied from our loadCookie utility */


      var loadCookie = function loadCookie(name, doNotParse) {
        var cookies = parseCookies(document.cookie);
        var cookieVal = cookies && cookies[name];

        if (typeof doNotParse === "undefined") {
          doNotParse = !cookieVal || cookieVal[0] !== "{" && cookieVal[0] !== "[";
        }

        if (!doNotParse) {
          try {
            cookieVal = JSON.parse(cookieVal);
          } catch (err) {
            /* Cannot serialize object */
          }
        }

        return cookieVal;
      };
      /* Add DMN cookie derived key-values to window object */


      window.keyValues.dnLoggedIn = isLoggedIn(loadCookie("DMN-P"));
      window.keyValues.isPremiumSubscriber = Boolean(loadCookie("DMN-SP"));
