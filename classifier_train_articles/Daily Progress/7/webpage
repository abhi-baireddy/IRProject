    #asset-0b1ff330-587e-50c9-a4a4-524e579c8a19 .collection-gallery-content-container .caption-container .img-responsive{
        width:100%
    }
    #asset-0b1ff330-587e-50c9-a4a4-524e579c8a19 .collection-gallery-content-container .caption-container .img-responsive:hover{
        cursor:default;
    }
