                <span class="sr-only">Loading...</span>\
            </div>\
        </div>';

    function searchWhiteout(){
        $('body').append(whiteOutDiv);
        $('.app-business .search-whiteout').show();
        $('.app-business .search-spinner-container').show();
        $('body').css({'overflow':'hidden'});
    }
    
    // disable empty inputs
    $('.business-refine-search-form').submit(function(e){
        e.preventDefault();

        $(this).find('.refine-range').each(function(){
            var $min = $(this).find('input.min');
            var $max = $(this).find('input.max');
            if ($max.val() > 0 && $min.val() == 0) {
                $min.val(1);
            }
        });

        $(this).find('input, select').each(function(){
            if ( this.value == '' || (this.type == 'checkbox' && $(this).prop('checked') == false) ) {
                $(this).prop('disabled', true);
            }
        });

        searchWhiteout();
        this.submit();
    });
    
