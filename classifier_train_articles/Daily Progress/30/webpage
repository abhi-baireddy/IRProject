    $('#business-search-750531 .business-search-submit').on(__tnt.client.clickEvent, function(){$('#business-search-750531').addClass('loading');$('#business-search-750531 .business-search-form').submit();
        return false;
    });
        
    $('#business-search-750531 .business-search-submit-filter').on(__tnt.client.clickEvent, function(){
        var keyword = $('#business-search-750531 .business-search-form input[name=keyword]').val();
        $('#business-search-750531 .business-search-form-filters input[name=keyword]').val(keyword);
        $('#business-search-750531').addClass('loading');
        $('#business-search-750531 .business-search-form-filters').submit();
        return false;
    });$('#business-search-750531').removeClass('loading');
