			function firefly_size_list_to_max_wh( size_list ) {
				let max_wh = [0, 0];
				for( let i = 0; i < size_list.length; i++ ) {
					if( size_list[i][0] > max_wh[0] ) max_wh[0] = size_list[i][0];
					if( size_list[i][1] > max_wh[1] ) max_wh[1] = size_list[i][1];
				}
				return max_wh;
