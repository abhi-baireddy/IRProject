  <script>
    tp = window.tp || [];
    tp.push(["setContentAuthor", "David Keene"]);         // The content author
    tp.push(["setContentSection", "Opinion"]);       // The content section
    tp.push(["setTags", ["Opinion", "Commentary"]]);                 // The content tags
