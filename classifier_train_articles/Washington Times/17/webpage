  <script>
    tp = window.tp || [];
    tp.push(["setContentAuthor", "Adam Zielonka"]);         // The content author
    tp.push(["setContentSection", "Sports"]);       // The content section
    tp.push(["setTags", ["Sports", "Basketball", "Maryland Terrapins", "NCAA", "Washington Mystics"]]);                 // The content tags
