  <section id="content" class="col-xs-31 col-lg-22 right-border">
    <div class="equal-heights-column-inner" style="padding-right: 15px; border-right: 1px solid #ccc;">
      <div class="content-inner">
        <!-- //#ad-lead-mobile -->
        <section class="full-article">
          <article style="margin-bottom: 0;">
            
            
            
            
            
            
            
            
              <!-- STN lead video -->
<div class='s2nPlayer k-NIOlh3JI' data-type='float'></div><script type='text/javascript' src='//embed.sendtonews.com/player3/embedcode.js?fk=NIOlh3JI&cid=4936&offsetx=0&offsety=0&floatwidth=400&floatposition=bottom-left&floatwidth=400' data-type='s2nScript'></script>
