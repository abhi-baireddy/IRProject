          <script>
            Promise.all([
              fetch("https://static01.nyt.com/newsgraphics/2020/03/16/coronavirus-maps/ebee1fc8a8f12d59180cc683750db9f998362177/data/timeseries/NYT-World.json").then(r => r.json())
            ]).then(function(data) {
              var target = document.querySelector('[data-component="334"]');
              var url = NYTG.ASSETS + '/build/js/table.js';
              var props = { amp: false, timeseries: data[0], location_name: "world", rows_default: "10", type: "countries", var_county_label: "county", var_county_label_plural: "counties" }

              
              let component;

              // TODO capture props. requires compilation in dev mode
              NYTG.watch(url, (mod) => {
                const Component = mod.default;

                try {
                  if (component) component.$destroy();
                  console.log('destroyed component 334');
                } catch (err) {
                  console.error(err);
                  target.innerHTML = '';
                }

                component = new Component({
                  target,
                  hydrate: !component,
                  props
                });
              });
            });
          </script>
          
