    line-height: 30px;
    margin-top: 5px;
    margin-bottom: 5px;
  }

  @media (min-width: 740px) {
    .g-inlineguide-list-header {
      font-size: 30px;
      line-height: 36px;
      margin-bottom: 10px;
      margin-top: 10px;
    }
  }

  .g-inlineguide-item-list {
    font-size: 15px;
    line-height: 20px;
    font-family: "nyt-franklin", arial, helvetica, sans-serif;
    font-weight: 500;
  }

  #g-inlineguide-item-list li {
    margin-bottom: 10px;
    padding-left: 15px;
    line-height: 20px;
  }

  @media (min-width: 740px) {
    #g-inlineguide-item-list li {
      line-height: 22px;
      font-size: 16px;
    }
  }

  #g-inlineguide-item-list li:before {
    color: #333333;
    margin-left: -15px;
    margin-right: 10px;
    top: 0;
