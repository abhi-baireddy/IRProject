  (function() {
    window.NYTG = window.NYTG || {};
    window.NYTG.ASSETS = "https://static01.nyt.com/newsgraphics/2020/04/11/apollo-13-as-they-shot-it/768e641a7ca9c65bc61a8a846274e17acd8d11a4/".slice(0, -1);
