      var coverasset = document.querySelector(".g-cover-asset");
      coverasset.appendChild(sharetools);

      var body = document.querySelector("#app").classList.add("has-top-cover");
  	})();
  </script>
  

  
  	<style id="nytg-dynamic-styles_top">
  		#top-wrapper { display: none; }
  		.css-16eycmb { margin-top: 0; }
  	</style>
  




