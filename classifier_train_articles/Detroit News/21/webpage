            (function () {
                window.privacyConfig = {
                    strictlyNecessary: true,
                    performance: true,
                    functional: true,
                    targeting: true,
                    socialMedia: true,
                    isCcpaSubject: false
                };
                var consentCookieName = 'OptanonConsent';
                if (document.cookie.indexOf(consentCookieName) !== -1) {
                    var catIds = {
                        1: 'strictlyNecessary',
                        2: 'performance',
                        3: 'functional',
                        4: 'targeting',
                        5: 'socialMedia'
                    };
                    function getCookie (name) {
                        var value = "; " + document.cookie;
                        var parts = value.split("; " + name + "=");
                        if (parts.length === 2) return parts.pop().split(";").shift();
                    }
                    var ocCookie = getCookie(consentCookieName);
                    if (ocCookie) {
                        ocCookie = decodeURIComponent(ocCookie);
                        var cookieParamGroups = ocCookie.split('&');
                        var cookieParams = {};
                        for (i=0; i<cookieParamGroups.length; i++) {
                            var splitGroup = cookieParamGroups[i].split('=');
                            if (splitGroup[1]) {
                                cookieParams[splitGroup[0]] = splitGroup[1];
                            }
                        }
                        if (cookieParams.groups) {
                            var groups = cookieParams.groups.split(',');
                            for (var i=0; i<groups.length; i++) {
                                var cat = groups[i].split(':');
                                var catId = parseInt(cat[0]);
                                if (catId) {
                                    var catName = catIds[catId];
                                    if (catName) {
                                        window.privacyConfig[catName] = !!parseInt(cat[1]);
                                    }
                                }
                            }
                        }
                        window.privacyConfig.isCcpaSubject = true;
                    }
                }

                // https://iabtechlab.com/wp-content/uploads/2019/11/US-Privacy-USER-SIGNAL-API-SPEC-v1.0.pdf
                var USPAPI_VERSION = 1;
                function getPrivacyString () {
                    if (!window.privacyConfig || !window.privacyConfig.isCcpaSubject) {
                        return '1---';
                    }
                    if (window.privacyConfig.targeting) {
                        return '1YNY';
                    }
                    return '1YYY';
                }
                function getuspdata (apiVersion, callback) {
                    if (typeof callback !== 'function') {
                        return console.error("__uspapi: callback parameter not a function");
                    }
                    if (apiVersion != USPAPI_VERSION) {
                        callback(null, false);
                        return;
                    }
                    var str = getPrivacyString();
                    if (!str) {
                        callback({version: null, uspString: null}, false);
                        return;
                    }
                    callback({
                        version: USPAPI_VERSION.toString(),
                        uspString: str
                    }, true);
                }
                function createLocatorFrame () {
                    if (!window.frames['__uspapiLocator']) {
                        if (document.body) {
                            var body = document.body;
                            var iframe = document.createElement('iframe');
                            iframe.style.cssText = 'display:none';
                            iframe.name = '__uspapiLocator';
                            body.appendChild(iframe);
                        } else {
                            setTimeout(createLocatorFrame, 5);
                        }
                    }
                }
                window.__uspapi = window.__uspapi || function (cmd) {
                    try {
                        return {
                            getUSPData: getuspdata,
                            __uspapi: function () {
                                return true;
                            }
                        }[cmd].apply(null, [].slice.call(arguments, 1));
                    } catch (err) {
                        console.error("__uspapi: Invalid command: ", cmd)
                    }
                };
                createLocatorFrame();
                function __handleUspapiMessage (event) {
                    var data = event && event.data && event.data.__uspapiCall;
                    if (data) {
                        window.__uspapi(data.command, data.version, (returnValue, success) => {
                            event.source.postMessage({
                                __uspapiReturn: {
                                    returnValue,
                                    success,
                                    callId: data.callId
                                }
                            });
                        });
                    }
                }
                window.addEventListener('message', __handleUspapiMessage, false);
            })();
