              });

              myStorage = window.sessionStorage;
              var key = myStorage.getItem('ksl-article-click');
              var purchase = {"actionField":{"id":46742133,"affiliation":"associated_press|","revenue":"Not Specified","tax":"Not Specified","shipping":"Not Specified","coupon":"article complete"},"products":[{"name":"US job losses mount as Trump presses plan to reopen business","id":46742133,"price":"Not Specified","brand":"KSL.com - News\/Content","category":"News|Business","variant":"associated_press|","quantity":1,"coupon":"article complete"}]};
              var storyId = 46742133;
              purchase.actionField.id = purchase.actionField.id + '|' + window.ddmPageviewId;
              if (key !== null) {
                var array = key.split('::');
                if (parseInt(array[0], 0) === storyId) {
                  purchase.actionField.affiliation = array[1];
                  purchase.products[0].id = array[0];
                  purchase.products[0].variant = array[1];
                  purchase.products[0].category = array[2];
                  kslEventBus.emit(kslEventBus.eventTypes.purchase, purchase);
                }
                myStorage.removeItem('ksl-article-click');
              }
            }
          });
        }());
      });
