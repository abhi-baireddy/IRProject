              var purchase = {"actionField":{"id":46742515,"affiliation":"internet_team|disaster","revenue":"Not Specified","tax":"Not Specified","shipping":"Not Specified","coupon":"article complete"},"products":[{"name":"Coronavirus updates: Trump pushes states to lift restrictions; projection says Utah should wait until June","id":46742515,"price":"Not Specified","brand":"KSL.com - News\/Content","category":"News","variant":"internet_team|disaster","quantity":1,"coupon":"article complete"}]};
              var storyId = 46742515;
              purchase.actionField.id = purchase.actionField.id + '|' + window.ddmPageviewId;
