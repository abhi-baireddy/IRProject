	var date_full = new Date();
	var day = date_full.getDate();
	var year = date_full.getFullYear();
	var monthsArray = ["January", "February", "March", "April", "May", "June", "July", "August", "September", "October", "November", "December"];
	var months = monthsArray[date_full.getMonth()];
	document.getElementById("baybriefing-date-today").innerHTML = months+" "+day+", "+year;
