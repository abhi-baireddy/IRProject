<article class="article">
    <header class="header"><!-- ferd/hearst/templates/premium/article/article_header.tpl -->
<!-- ferd/hearst/templates/premium/article/article_headline.tpl -->
                        <div class="header-subsection kicker">
                Local                 <span class="light"> // </span>                Bay Area & State            </div>
                <h1 itemprop="headline" class="header-title">Man survives being hit by Caltrain in SF</h1>
<!-- e ferd/hearst/templates/premium/article/article_headline.tpl --> <div class="header-author-img-time">
            <div class="header-author-img" data-progressive="true" data-component="photo">
        <a href="//www.sfchronicle.com/author/steve-rubenstein/" title="Steve Rubenstein" rel="author" itemprop="url" class="hdn-analytics" data-hdn-analytics="visit|Steve Rubenstein-Author Page|article-header|1" target="_blank" rel="noopener noreferrer"><img data-src="https://s.hdnux.com/photos/36/30/00/7962653/5/square_small.jpg" data-src-4x="https://s.hdnux.com/photos/36/30/00/7962653/5/square_medium.jpg" alt="Photo of Steve Rubenstein"/></a>    </div>
        <div class="header-author-time h5">
        <span class="header-byline" itemscope itemtype="http://schema.org/Person">
                                                                <a href="//www.sfchronicle.com/author/steve-rubenstein/" title="Steve Rubenstein" rel="author" itemprop="url" class="header-authors-url hdn-analytics" data-hdn-analytics="visit|Steve Rubenstein-Author Page|article-header|2" target="_blank" rel="noopener noreferrer">
                             <span itemprop="name" class="header-authors-name">Steve Rubenstein</span>
                                            </a>                                                        </span>
        <time datetime="2020-04-18T22:27:47+00:00" itemprop="datePublished">
            April 18, 2020
        </time>
            </div>
</div>
<div class="header-social shareTools">
    <!-- ferd/hearst/templates/premium/common/share_tools.tpl -->
<div class="share-list">
    <a class="share-facebook" data-shareNetwork="facebook">Facebook</a>
