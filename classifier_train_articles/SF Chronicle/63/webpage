<!-- ferd/hearst/templates/premium/article/article_headline.tpl -->
                        <div class="header-subsection kicker">
                Food                                            </div>
                <h1 itemprop="headline" class="header-title">Curry Hyuga and other new Bay Area restaurants open for takeout during coronavirus</h1>
<!-- e ferd/hearst/templates/premium/article/article_headline.tpl --> <div class="header-author-img-time">
            <div class="header-author-img" data-progressive="true" data-component="photo">
        <a href="//www.sfchronicle.com/author/janelle-bitker/" title="Janelle Bitker" rel="author" itemprop="url" class="hdn-analytics" data-hdn-analytics="visit|Janelle Bitker-Author Page|article-header|1" target="_blank" rel="noopener noreferrer"><img data-src="https://s.hdnux.com/photos/01/00/77/51/17037949/3/square_small.jpg" data-src-4x="https://s.hdnux.com/photos/01/00/77/51/17037949/3/square_medium.jpg" alt="Photo of Janelle Bitker"/></a>    </div>
        <div class="header-author-time h5">
        <span class="header-byline" itemscope itemtype="http://schema.org/Person">
                                                                <a href="//www.sfchronicle.com/author/janelle-bitker/" title="Janelle Bitker" rel="author" itemprop="url" class="header-authors-url hdn-analytics" data-hdn-analytics="visit|Janelle Bitker-Author Page|article-header|2" target="_blank" rel="noopener noreferrer">
                             <span itemprop="name" class="header-authors-name">Janelle Bitker</span>
                                            </a>                                                        </span>
        <time datetime="2020-04-15T20:35:33+00:00" itemprop="datePublished">
            April 15, 2020
        </time>
                <time datetime="2020-04-15T20:37:35+00:00" itemprop="dateModified">
            Updated: April 15, 2020 1:37&nbsp;p.m.
        </time>
            </div>
</div>
