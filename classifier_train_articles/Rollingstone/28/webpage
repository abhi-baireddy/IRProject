		data-list-title="Neil Young: &#8216;Special Deluxe&#8217; (2014)"
		data-list-permalink="https://www.rollingstone.com/music/music-lists/books-greatest-rock-memoirs-of-all-time-161198/neil-young-special-deluxe-cars-dogs-971183/"
		data-list-item-id="971183"
		data-list-item-authors="admin">

	<figure class="c-list__picture">
		<div class="c-list__share" data-collapsible="collapsed" data-collapsible-close-on-click>
			<div class="c-list__social-bar" data-collapsible-panel>
				
<ul class="c-social-bar c-social-bar--small">

			
			<li class="c-social-bar__item">
				<a href="https://www.facebook.com/sharer.php?u=https://www.rollingstone.com/music/music-lists/books-greatest-rock-memoirs-of-all-time-161198/neil-young-special-deluxe-cars-dogs-971183/&#038;title=Neil%20Young:%20&#8216;Special%20Deluxe&#8217;%20(2014)&#038;sdk=joey&#038;display=popup&#038;ref=plugin&#038;src=share_button" class="c-social-bar__link" title="Facebook" rel="noopener noreferrer" target="_blank">
