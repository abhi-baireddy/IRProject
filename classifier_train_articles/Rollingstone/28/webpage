<article class="c-list__item"
		id="list-item-13"
		data-list-item="13"
		data-list-title="John Taylor: &#8216;In the Pleasure Groove&#8217; (2012)"
		data-list-permalink="https://www.rollingstone.com/music/music-lists/books-greatest-rock-memoirs-of-all-time-161198/john-taylor-pleasure-groove-971205/"
		data-list-item-id="971205"
		data-list-item-authors="admin">

	<figure class="c-list__picture">
		<div class="c-list__share" data-collapsible="collapsed" data-collapsible-close-on-click>
			<div class="c-list__social-bar" data-collapsible-panel>
				
<ul class="c-social-bar c-social-bar--small">

			
			<li class="c-social-bar__item">
				<a href="https://www.facebook.com/sharer.php?u=https://www.rollingstone.com/music/music-lists/books-greatest-rock-memoirs-of-all-time-161198/john-taylor-pleasure-groove-971205/&#038;title=John%20Taylor:%20&#8216;In%20the%20Pleasure%20Groove&#8217;%20(2012)&#038;sdk=joey&#038;display=popup&#038;ref=plugin&#038;src=share_button" class="c-social-bar__link" title="Facebook" rel="noopener noreferrer" target="_blank">
					<span class="c-icon c-icon--white">
						<svg>
