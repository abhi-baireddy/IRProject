		data-list-title="Brian Wilson: &#8216;I Am Brian Wilson&#8217; (2016)"
		data-list-permalink="https://www.rollingstone.com/music/music-lists/books-greatest-rock-memoirs-of-all-time-161198/brian-wilson-i-am-brian-wilson-971180/"
		data-list-item-id="971180"
		data-list-item-authors="admin">

	<figure class="c-list__picture">
		<div class="c-list__share" data-collapsible="collapsed" data-collapsible-close-on-click>
