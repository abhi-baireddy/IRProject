		data-list-item-id="971224"
		data-list-item-authors="admin">

	<figure class="c-list__picture">
		<div class="c-list__share" data-collapsible="collapsed" data-collapsible-close-on-click>
			<div class="c-list__social-bar" data-collapsible-panel>
				
<ul class="c-social-bar c-social-bar--small">

			
			<li class="c-social-bar__item">
				<a href="https://www.facebook.com/sharer.php?u=https://www.rollingstone.com/music/music-lists/books-greatest-rock-memoirs-of-all-time-161198/bruce-springsteen-born-to-run-book-971224/&#038;title=Bruce%20Springsteen:%20&#8216;Born%20to%20Run&#8217;%20(2016)&#038;sdk=joey&#038;display=popup&#038;ref=plugin&#038;src=share_button" class="c-social-bar__link" title="Facebook" rel="noopener noreferrer" target="_blank">
