		data-list-item="36"
		data-list-title="Joe Boyd: &#8216;White Bicycles&#8217; (2006)"
		data-list-permalink="https://www.rollingstone.com/music/music-lists/books-greatest-rock-memoirs-of-all-time-161198/joe-boyd-white-bicycles-971177/"
		data-list-item-id="971177"
		data-list-item-authors="admin">

	<figure class="c-list__picture">
		<div class="c-list__share" data-collapsible="collapsed" data-collapsible-close-on-click>
			<div class="c-list__social-bar" data-collapsible-panel>
				
<ul class="c-social-bar c-social-bar--small">

			
			<li class="c-social-bar__item">
				<a href="https://www.facebook.com/sharer.php?u=https://www.rollingstone.com/music/music-lists/books-greatest-rock-memoirs-of-all-time-161198/joe-boyd-white-bicycles-971177/&#038;title=Joe%20Boyd:%20&#8216;White%20Bicycles&#8217;%20(2006)&#038;sdk=joey&#038;display=popup&#038;ref=plugin&#038;src=share_button" class="c-social-bar__link" title="Facebook" rel="noopener noreferrer" target="_blank">
