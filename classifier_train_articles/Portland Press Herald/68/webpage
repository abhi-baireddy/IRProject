<script>
(function($, undefined){
   'use strict';

   var cookie = 'MSPAN_Paywall_user';

   $(init);

   function init(){
      var user;

      if( typeof Paywall === 'undefined' ){
         return;
      }

      user = Paywall.Cookie.get(cookie);

      try{
         user = JSON.parse(user);
      }catch(er){
         return;
      }
      if( window.location.search.match(/pp-update-subscription/i) ){
         user.rate_code = user.rate_code + '_updated';
         Paywall.Cookie.set(cookie, user);
      }
   }

   return {};
}(jQuery));
